# Sea Dog Baron bounce event
character_event = {
	id = ko.1013

	is_triggered_only = yes
	trigger = {
		has_character_modifier = port_of_calling 
	}
	hide_window = yes

	immediate = {
		if = {
			limit = {
				event_target:fishingvillage = {
					owner = { diplomacy = 8 }
				}
			}
			event_target:fishingvillage = {
				owner = { 
					character_event = { id = ko.1014 }
				}
			} 
		}
	}
}

