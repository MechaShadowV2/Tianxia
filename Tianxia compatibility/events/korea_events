# Lazy Family member makes it up the mountain
character_event = {
	id = ko.1074
	desc =  EVTDESC_KO_1014
	picture = GFX_evt_mountains
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_1014
		custom_tooltip = { text = EVTOPTA_KO_1014_TT }		
 		random_list = {
			50 = {
				remove_trait = slothful
			}
			50 = {}
		}		
		hidden_tooltip = {
			any_courtier = {
				limit = {
					is_inaccessible_trigger = no
					opinion = { who = ROOT value = 10 }  
					family_of_root_trigger = yes
				} 
				opinion = {
					modifier = opinion_impressed
					who = ROOT
					years = 5
				}		
			}		
		}
	}
}

# Father slayed by tiger
character_event = {
	id = ko.1150
	desc =  EVTDESC_KO_035.1
	picture = GFX_evt_tiger_hunt_bad
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_035.1 # This has no visible effect; why?
		trigger = { has_global_flag = tiger_killings }
		custom_tooltip = { text = EVTOPTA_KO_035.1_TT }		
		clr_global_flag = tiger_killings
	}		

	option = {
		name = EVTOPTB_KO_035.1
		character_event = { id = ko.031 days = 365 }
		random_realm_lord = { # This has no effect and a rather bad trigger; fix!
			limit = {
				martial = 1
				martial = 2
				martial = 3
				martial = 4
				martial = 5
			}
		}
 		custom_tooltip = { text = EVTOPTA_KO_035.1_TT }		
		clr_global_flag = tiger_killings
	}
}