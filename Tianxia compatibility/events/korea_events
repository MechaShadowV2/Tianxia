		name = EVTOPTB_KO_1005
		random_child = {
			limit = {
				religion = buddhist
				is_alive = yes
				age = 6
				NOT = { age = 18 }

			}
			opinion = {
				modifier = opinion_kind_pos
				who = ROOT
				years = 2
			}	
		}
		hidden_tooltip = {
			narrative_event = { id = ko.1066 }
		}
	}
	
	option = {
		name = EVTOPTC_KO_1001 # Why would you ever pick this option?
		hidden_tooltip = {
			narrative_event = { id = ko.1066 }
		}
	}
}

