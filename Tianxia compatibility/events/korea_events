# Child pushes you
character_event = {
	id = ko.1048
	desc =  EVTDESC_KO_902.6
	picture = GFX_evt_noble_haughty_talking_to_peasants

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_902.6
		add_trait = kind
	}
	option = {
		name = EVTOPTB_KO_902.6 # This does nothing; why?
		trigger = { trait = wroth }
	}
	option = {
		name = EVTOPTC_KO_902.6
		add_trait = cruel # There is no AI weight preventing a Kind character from picking this; fix that!
	}		
}

# Medical Cookies
