		name = EVTOPTB_KO_569.4
		tooltip_info = diplomacy
		trigger = { diplomacy = 10 }
		any_realm_character = {
			limit = {
				is_councillor = yes # Not voter, and not just on ROOT's council?
			} 
			character_event = { id = ko.853	}
		}
	}
	option = {
		name = EVTOPTC_KO_569.4
		tooltip_info = intrigue
		job_treasurer = {
			reverse_opinion = {
				who = ROOT
				modifier = opinion_lying_about_confession
				months = 70
			}			
			reverse_opinion = {
				who = FROM # Is this ROOT?
				modifier = opinion_charge_traitor
				months = 24
			}
		}
		any_realm_character = {
			limit = {
				is_councillor = yes # Not voter, and not just on ROOT's council?
			} 
			character_event = { id = ko.854	}
		}
	}							
	option = {
		name = EVTOPTD_KO_569.4
		prestige = -500 # This is huge; is it justifiable?
		any_realm_character = {
			limit = {
				is_councillor = yes # Not voter, and not just on ROOT's council?
			}
			opinion = {
				who = ROOT
				modifier = opinion_lying_about_confession
				months = 70
			}
			character_event = { id = ko.852	}
		}
	}		
}

