# Your dynasty is false!
character_event = {
	id = ko.300
	desc = EVTDESC_KO_320
	picture = GFX_evt_Council_Around_Table
	capable_only = yes
	prisoner = no
	only_playable = yes
	
	is_triggered_only = yes
	
	trigger = {
		dynasty = 1000027100 # Why is there a dynasty-specific event?
		# This can trigger several times, and also if the dynasty has ended up in a completely different part of the map or if the situation near Korea is completely differe; is that reasonable?
	}
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = shy
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
	}	
	
	option = {
		name = EVTOPTA_KO_320
		hidden_tooltip = {
			random_list = {
				50 = {
					character_event = { id = ko.301 }
				}
				50 = {}
			}
		}
	}	
	option = {
		name = EVTOPTB_KO_320
		prestige = -50
		any_vassal = {
			opinion = {
				modifier = opinion_unimpressed
				who = ROOT
				years = 5
			}	
		}	
	}
	
	option = {
		name = EVTOPTC_KO_320 # This has no effect; why?
		tooltip_info = cynical
		trigger = { trait = cynical }
	}
		
	
	option = {
	tooltip_info = intrigue
	trigger = { intrigue = 15 }
	name = EVTOPTD_KO_320
	prestige = 50
	}
}

# Your dynasty is false! 2

character_event = {
	id = ko.301
	desc = EVTDESC_KO_321.1
	picture = GFX_evt_judged_by_ruler
	capable_only = yes
	prisoner = no
	only_playable = yes
	
	is_triggered_only = yes
	
	immediate = {
		random_courtier = {
			limit = {
				OR = {
					is_foe = ROOT
					has_focus = focus_intrigue
					learning = 12
					intrigue = 10
				}
			}
			save_event_target_as = go_dynasty_liar
		}
		random_courtier = {
			limit = {
				OR = {
					is_foe = ROOT
					has_focus = focus_intrigue
					learning = 12
					intrigue = 10
					diplomacy = 10
				}
			}
			save_event_target_as = go_dynasty_liartwo
		}
	}	
	
	option = {
		name = EVTOPTA_KO_321.1
		any_vassal = {
			opinion = {
				modifier = opinion_tyrant
				who = ROOT
				years = 15 # Why fifteen years?
			}	
		}
		
		# TODO: Grace penalties!
		
		event_target:go_dynasty_liar  = {
			death = {
				death_reason = death_execution_beheading
				# No killer has been set!
			}
		}
		event_target:go_dynasty_liartwo  = {
			death = {
				death_reason = death_execution_beheading
				# No killer has been set!
			}
		}
	}
			
	option = {
		name = EVTOPTB_KO_321.1
		any_vassal = {
			limit = {
				is_friend = event_target:go_dynasty_liar
				is_close_relative = event_target:go_dynasty_liar
				# Is there an OR missing here?
				# Why isn't go_dynasty_liartwo's relatives/friends upset?
			} 
			opinion = {
				modifier = opinion_tyrant
				who = ROOT
				years = 5 # Why five years?
			}	
		}
		event_target:go_dynasty_liar  = {
			imprison = ROOT
		}
		event_target:go_dynasty_liartwo  = {
			imprison = ROOT
		}
	}
	
	option = {
		name = EVTOPTC_KO_321.1
		trigger = { trait = cynical } 
		any_vassal = {
			limit = {
				NOT = {
					trait = cynical
				}
			}
			opinion = {
				modifier = opinion_honorable
				who = ROOT
				years = 5
			}	
		}
		any_courtier = {
			limit = {
				is_adult = no # Why don't adults care?
			}
			opinion = {
				modifier = opinion_honorable
				who = ROOT
				years = 5
			}	
		}		
	}
	option = {
		name = EVTOPTD_KO_321.1
		tooltip_info = just
		trigger = { trait = just }
		prestige = 50	
	}
}

	
