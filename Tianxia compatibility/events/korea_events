# Meet Guru
character_event = {
	id = ko.088
	desc = EVTDESC_KO_059
	picture = GFX_evt_roi_emissary
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_059
		custom_tooltip = {
			text = EVTOPTA_KO_059_TT
		}
		scaled_wealth = -0.2
		random_list = {
			70 = {}
			30 = {
				add_trait = mystic 
			}
		}
	}
	option = {
		name = EVTOPTB_KO_059
		tooltip_info = learning
		trigger = { learning = 14 }
		character_event = { id = ko.091 days = 100 }
		scaled_wealth = -0.2
		any_vassal = {
			limit = {
				OR = {
					religion = korean_pagan_reformed
					religion = taoist
				}
			}
			opinion = {
				modifier = opinion_disappointed
				who = ROOT
				years = 10
			   }
		}
		random_list = {
			70 = {}
			30 = {
				character_event = { id = ko.090 days = 60 }
			}
		}
	}
		
	option = {
		name = EVTOPTC_KO_059
		tooltip_info = learning
		trigger = { learning = 18 }
		character_event = { id = ko.092 days = 100 }
		scaled_wealth = -0.2
		any_vassal = {
			limit = { 
				religion = korean_pagan_reformed
			}
			opinion = {
				modifier = opinion_disappointed
				who = ROOT
				years = 10
			   }
		}
		random_list = {
			70 = {}
			30 = {
				character_event = { id = ko.090 days = 55 }
			}
		}
	}	
	option = {
		name = EVTOPTD_KO_059
		event_target:language_guru  = {
			banish = yes
		}
	}
}

# Stresses
character_event = {
	id = ko.089
	desc = EVTDESC_KO_060
	picture = GFX_evt_scribe
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_060
		change_learning = 1
	}
	
	option = {
		name = EVTOPTB_KO_060
		random_list = {
			40 = {}
			20 = {
				wealth = -20
			}
			20 = {
				add_trait = diligent
			}
			20 = {
				add_trait = stressed
			}
		}
	}
}
	
# Nobles come in to hi-jack process.
character_event = {
	id = ko.090
	# Multiple descs cannot be correct as written; fix this!
	desc = EVTDESC_KO_056.1
	desc = EVTDESC_KO_056.2
	desc = EVTDESC_KO_056.3
	desc = EVTDESC_KO_056.4
	
	picture = GFX_evt_scribe
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_056
		trigger = { learning = 12 }
		prestige = -200
		if = {
			limit = {
				among_most_powerful_vassals = 5
			}
			any_realm_lord = {
				limit = {
					prisoner = no
					war = no
					in_command = no
					NOT = { trait = incapable }
					age = 16
				}
				opinion = {
					modifier = opinion_debate_participant
					who = ROOT
					years = 10
				}
			}
		}
	}
	
	option = {
		name = EVTOPTB_KO_056
		trigger = { learning = 14 }
		prestige = -100
		if = {
			limit = {
				among_most_powerful_vassals = 5
			}
			any_realm_lord = { 
				limit = {
					prisoner = no
					war = no
					in_command = no
					NOT = { trait = incapable }
					age = 16
				}
				opinion = {
					modifier = opinion_reasonable
					who = ROOT
					years = 10
			    }		
			}
		}
	}
	option = {
		name = EVTOPTC_KO_056
		change_learning = -6 # This is a MASSIVE penalty (the average stat value tends to be around 8-10, excluding artifact bonuses); is this reasonable at all?
		if = {
			limit = {
				among_most_powerful_vassals = 5
			}
			any_realm_lord = { 
				limit = {
					prisoner = no
					war = no
					in_command = no
					NOT = { trait = incapable }
					age = 16
				}
				opinion = {
					modifier = opinion_debate_winner
					who = ROOT
					years = 10
			    }		
			}
		}
	}
	option = {
		name = EVTOPTD_KO_056 # This option seems incomplete since nothing happens...
		tooltip_info = intrigue
		trigger = { intrigue = 10 }
	}
}

# Decide to adopt an Indian style script
