#Reform the alphabet		
narrative_event = {
	id = ko.081
	desc = EVTDESC_KO_051
	title = EVTTITLE_KO_051
	picture = GFX_evt_scholars
	
	prisoner = no
	only_playable = yes
	culture_group = koreanic
	min_age = 20
	is_triggered_only = yes
	trigger = {
		has_landed_title = e_korea # We might want to check the kingdoms, or something else, but k_korea was obviously wrong since that no longer exists
		NOT = {
			has_global_flag = k_alphabet_reformed
		}
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 0.5
			trait = slothful   
		}
		modifier = {
			factor = 3
			trait = diligent  
		}
		modifier = {
			factor = 2
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 5
			trait = scholar
		}
		modifier = {
			factor = 2
			trait = proud
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			has_focus = focus_scholarship
		}
	}
	
	option = {
		name = EVTOPTA_KO_051
		
		character_event = { id = ko.082 days = 70 }								
	}
	option = {
		name = EVTOPTB_KO_051
		job_spiritual = {
			opinion = {
				modifier = opinion_great_minds
				who = ROOT
				years = 3
			}						
		}
	}
	option = {
		name = EVTOPTC_KO_051	
		random_list = {
			10 = {
				add_trait = cynical
			}
			90 = {
				prestige = 20
			}
		}
	}
}

# Let the experiment begin
