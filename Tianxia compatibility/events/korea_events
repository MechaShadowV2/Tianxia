	id = ko.742
	desc = EVTDESC_KO_415
	picture = GFX_evt_daughter_asking_father
	only_playable = yes

	is_triggered_only = yes
	trigger = {
		has_character_flag = daughter_marshal_kdrama # This flag can be set for non-adults and lacks proper attraction checks where it is set; fix this!
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = shy
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
	}
	
	option = {
		name = EVTOPTA_KO_415
		
		# The variable changes probably should be hidden...
		change_variable = { which = "martial_suggestion" value = 2 }
		change_variable = { which = "their_luv" value = 1 }
		job_marshal = {
			opinion = {
				modifier = opinion_agreed_with_opinion
				who = ROOT
				years = 1
			}	
		}		
 	}
	option = {
		name = EVTOPTB_KO_415 # This has no real visible effect (the variables don't tell you anything)...
		
		# The variable changes probably should be hidden...
		change_variable = { which = "martial_suggestion" value = -1 }
		change_variable = { which = "their_luv" value = -1 }		
 	}
	
	option = {
		name = EVTOPTC_KO_415 # This has no real visible effect (the variables don't tell you anything)...
		
		# The variable changes probably should be hidden...
		change_variable = { which = "their_luv" value = -1 }		
	}
}	
	
