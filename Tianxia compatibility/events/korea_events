# Sodo Tree
character_event = {
	id = ko.268
	desc = EVTDESC_KO_113.w
	picture = GFX_evt_market_sinic
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		NOT = {
			has_character_flag = explained_sodo
		}
		# Is this strict enough?
		OR = {
			has_landed_title = d_gyeonggi
			has_landed_title = c_hanseong
			has_landed_title = c_namyangju
			has_landed_title = c_chunju
			has_landed_title = c_hwaseong
		}
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = shy
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
	}
	
	option = {
		name = EVTOPTA_KO_113.w
		set_character_flag = explained_sodo
		clr_global_flag = banned_sodo_tree
		piety = 20
		random_list = { 
			75 = {} 
			25 = {
				add_trait = zealous
			} 
		}
		random_child = {
			opinion = {
				modifier = opinion_disappointed
				who = ROOT
				years = 1
			}
		}
	}
	option = {
		name = EVTOPTB_KO_113.w
		set_character_flag = explained_sodo
		set_global_flag = banned_sodo_tree
		random_demesne_province = {
			limit = {
				is_capital = yes # Why do we scope to a random province if we're going for the capital all the time?
			}
			add_province_modifier = {
				name = thieves_guild
				duration = 600
			}
		}
		random_child = {
			opinion = {
				modifier = opinion_delighted
				who = ROOT
				years = 1
			}
		}

	}
	option = {
		name = EVTOPTC_KO_113.w
		set_character_flag = explained_sodo
		random_child = {
			add_trait = cynical
		}
	}	
}

# Sodo Tree Decoration
character_event = {
	id = ko.269
	desc = EVTDESC_KO_113.x
	picture = GFX_evt_market_sinic
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		NOT = {
			has_character_flag = sodo_decoration
		}
		# Is this strict enough?
		OR = {
			has_landed_title = d_gyeonggi
			has_landed_title = c_hanseong
			has_landed_title = c_namyangju
			has_landed_title = c_chunju
			has_landed_title = c_hwaseong
			is_father = yes
		}
	}
	
	immediate = {
		any_child  = {
			limit = {
				is_alive = yes
				is_female = yes
			}
			save_event_target_as = sodo_helper
		}
	}
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = shy
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
	}
	
	option = {
		name = EVTOPTA_KO_113.x
		set_character_flag = sodo_decoration
		piety = 20
		random_list = { 
			75 = {} 
			25 = {
				add_trait = zealous
			} 
		}
		event_target:sodo_helper = {
			opinion = {
				modifier = opinion_delighted
				who = ROOT
				years = 5
			}
			random_list = { 
				50 = {}
				25 = {
					add_trait = proud
				} 	
				25 = {
					add_trait = zealous
				} 
			}
		}
		any_courtier = { # Not random?
			opinion = {
				modifier = clan_propaganda_effort
				who = event_target:sodo_helper
				years = 5
			}		
		}	
	}
	option = {
		name = EVTOPTB_KO_113.x
		set_character_flag = sodo_decoration
		piety = 20
	}
	option = {
		name = EVTOPTC_KO_113.x
		trigger = { NOT = { has_global_flag = banned_sodo_tree } }
		set_character_flag = sodo_decoration
		random_child = {
			add_trait = cynical
		}
	}	
}

