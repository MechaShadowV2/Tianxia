# Swing discovery
character_event = {
	id = ko.519
	desc = EVTDESC_KO_518.1
	picture = GFX_evt_garden

	is_triggered_only = yes
	
	# TODO: Grace penalty if relevant...
	
	option = {
		name = EVTOPTA_KO_518.1
		random_backed_character = {
			limit = {
				OR = {
					has_plot = plot_kill_character
					has_plot = plot_kill_spouse
				}
				NOT = { has_character_flag = event7041 }
				NOT = {
					plot_target_char = {
						any_known_plotter = {	character = PREVPREV }
					}
				}
			}
			opinion = {
				who = ROOT
				modifier = revealed_plot
				months = 120
			}
			set_character_flag = event7041
			hidden_tooltip = {
				character_event = { id = 7046 days = 1 }
			}
		}
	}
}

# Swing discovery
character_event = {
	id = ko.520
	desc = EVTDESC_KO_518.2
	picture = GFX_evt_garden

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_518.2
		job_spiritual = {
			# This cannot be correct; it would add ALL opposite-sex countiers of the LS as their lover, ignore homosexuality on either party, and ignore stuff like being in hiding...
			any_courtier = {
				limit = {
					is_adult = yes
					is_opposite_sex = ROOT
					NOT = { trait = incapable }
				}
				add_lover = PREV
			}
		}
		piety = 20
	}
}

