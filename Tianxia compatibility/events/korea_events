# Inform Steward about organizing Circle Dance
character_event = {
	id = ko.862
	desc = EVTDESC_KO_571
	picture = GFX_evt_small_town_sinic

	is_triggered_only = yes
	
	immediate = {
		if = {
			limit = {
				liege = {
					capital_scope = {
						has_province_modifier = depopulated_1
					}
				}
			}
			set_variable = { which = "circle_quality" value = -1 }
		}
		if = {
			limit = {
				liege = {
					capital_scope = {
						has_province_modifier = depopulated_2
					}
				}
			}
			set_variable = { which = "circle_quality" value = -2 }
		}
		if = {
			limit = {
				liege = {
					capital_scope = {
						has_province_modifier = depopulated_3
					}
				}
			}
			set_variable = { which = "circle_quality" value = -3 }
		}
		if = {
			limit = {
				liege = {
					capital_scope = {
						has_province_modifier = prosperity_modifier_1
					}
				}
			}
			set_variable = { which = "circle_quality" value = 1 }
		}
		if = {
			limit = {
				liege = {
					capital_scope = {
						has_province_modifier = prosperity_modifier_2
					}
				}
			}
			set_variable = { which = "circle_quality" value = 2 }
		}
		if = {
			limit = {
				liege = {
					capital_scope = {
						has_province_modifier = prosperity_modifier_3
					}
				}
			}
			set_variable = { which = "circle_quality" value = 3 }
		}
	}
	
	option = {
		name = EVTOPTA_KO_571
		character_event = { id = ko.863 }			
	}			
}		

# Pick Location
