# Shaman seeking rattle asks for money
character_event = {
	id = ko.1057
	
	# Multiple descs without triggers; fix this!
	
	desc =  EVTDESC_KO_908.5
	desc =  EVTDESC_KO_908.51
	desc =  EVTDESC_KO_908.52
	desc =  EVTDESC_KO_908.53
	desc =  EVTDESC_KO_908.54
	desc =  EVTDESC_KO_908.55
	
	picture = GFX_evt_mu_rattle
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_908.5
		scaled_wealth = -0.2
		custom_tooltip = { text = EVTOPTA_KO_908.5_TT }
		hidden_tooltip = {
			random_list = {
				60 = {
					character_event = { id = ko.1058 }
				}
				40 = {
					character_event = { id = ko.1059 }
				}
			}
		}
	}

	option = {
		name = EVTOPTB_KO_908.5
		custom_tooltip = { text = EVTOPTB_KO_908.5_TT }
		scaled_wealth = -0.4
		hidden_tooltip = {
			random_list = {
				60 = {
					character_event = { id = ko.1058 }
				}
				40 = {
					character_event = { id = ko.1059 }
				}
			}
		}
	}
	option = {
		name = EVTOPTC_KO_908.5
		custom_tooltip = { text = EVTOPTC_KO_908.5_TT }
		scaled_wealth = -0.6
		hidden_tooltip = {
			random_list = {
				60 = {
					character_event = { id = ko.1058 }
				}
				40 = {
					character_event = { id = ko.1059 }
				}
			}
		}
	}
	option = {
		name = EVTOPTD_KO_908.5
		custom_tooltip = { text = EVTOPTD_KO_908.5_TT }
		piety = 15
		trigger = {
			NOR = {
				religion = korean_pagan
				religion = korean_pagan_reformed
				religion = shinto
			}
		}		
	}	
}

