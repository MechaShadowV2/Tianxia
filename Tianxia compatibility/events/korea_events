# They dead 2 (bad military victorys)
character_event = {
	id = ko.985
	desc = EVTDESC_KO_582.9
	picture = GFX_evt_battle

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.9
		prestige = 1 # This is a pointless reward!
		owner = { # Wasn't it a demesne province, making ROOT the owner?
			opinion = {
				modifier = opinion_upset
				who = ROOT
				years = 1
			}
		}
		random_list = {
			10 = {
				change_martial = 1
			}
			90 = {}
		}
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_2 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}			
	}
	option = {
		name = EVTOPTB_KO_582.9
		wealth = -120 # This is incredibly expensive, considering what you get...
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}
		event_target:fishingvillage = {
			owner = {
				spawn_unit = {
					owner = ROOT
					province = ROOT
					home = ROOT
					troops = {
						archers = { 100 500 }
						light_infantry = { 100 200 }
					}
					attrition = 0.3
					maintenance_multiplier = 0.5
					reinforces = yes
					reinforce_rate_multiplier = 0.33
				}
			}
		}
	}
	option = {
		name = EVTOPTC_KO_582.9	
		wealth = -50
	}
	option = {
		name = EVTOPTD_KO_582.9
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = no
			age = 8
			trait = cynical
			trait = stressed
		}
		new_character = {
			opinion = {
				modifier = opinion_loyal_servant
				who = ROOT
				years = 30
			}		
		}			
		wealth = -5
	}		
}

# Fight at duel
character_event = {
	id = ko.986
	desc = EVTDESC_KO_582.10
	picture = GFX_evt_Knights_melee_battle_han

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.10
		# If a character has combat rating X+Y (Y>0), they also have combat rating X; this means good combatants get increased chance for worse options too!
		random_list = {
			10 = {
				modifier = {
					factor = 1 # This does nothing!
					combat_rating = 0
				} 
				character_event = { id = ko.987 } #get knocked out
			}
			10 = {
				modifier = {
					factor = 2
					combat_rating = 1
				} 
				character_event = { id = ko.988 } #scar
			}
			10 = {
				modifier = {
					factor = 3
					combat_rating = 2
				} 
				character_event = { id = ko.988 } #friend helps you
			}
			10 = {
				modifier = {
					factor = 4
					combat_rating = 3
				}
				character_event = { id = ko.987 } #chop off arm
			}
		}
	}
	option = {
		name = EVTOPTB_KO_582.10
		add_trait = craven
	}
}

# Fight at duel
character_event = {
	id = ko.987
	desc = EVTDESC_KO_582.11
	picture = GFX_evt_Knights_melee_battle_han

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.11
		create_character = { # Is it guaranteed that a Shinto Japanese pirate is reasonable at ROOT's location; for example, what if the fishing village is in Rome?
			random_traits = yes
			dynasty = NONE
			religion = shinto
			culture = japanese
			female = no
			age = 24
			trait = scarred
			trait = quick
			trait = cynical
			attributes = {
				martial = 10
			}
		}
		new_character = {
			imprison = ROOT
		}
		hidden_tooltip = {
			random_list = {
				25 = { character_event = { id = ko.985 } }
				25 = { character_event = { id = ko.982 } }
				25 = { character_event = { id = ko.993 } }
				25 = { character_event = { id = ko.974 } }
			} 
		}		
	}
	option = {
		name = EVTOPTB_KO_582.11 # The random events are the same as in A; why would you pick a random Lowborn prisoner over 25 prestige?
		prestige = 25
		hidden_tooltip = {
			random_list = {
				25 = { character_event = { id = ko.985 } }
				25 = { character_event = { id = ko.982 } }
				25 = { character_event = { id = ko.993 } }
				25 = { character_event = { id = ko.974 } }
			} 
		}
	}
}

