# Slave Story 3
narrative_event = {
	id = ko.409
	desc = EVTDESC_KO_178
	title = EVTTITLE_KO_178	
	picture = GFX_evt_humiliation_prisoner

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_178
		prisoner_humiliation_effect = yes
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
			random_child = {
				random_list = {
					10 = {
						add_trait = cruel
					}
					90 = {}
				}
			} 	 		
		}
	}
	option = {
		remove_trait = kind
		name = EVTOPTB_KO_178
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			} 
		}
	}
	option = {
		add_trait = kind
		remove_trait = cruel
		name = EVTOPTC_KO_178
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		tooltip_info = lunatic
		trigger = { trait = lunatic }
		prestige = 500
		add_trait = kind
		add_character_modifier = {
			name = respected_by_the_people	
			duration = 365
		} 
		name = EVTOPTD_KO_178
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}

# Not sure what triggers the below chain, but make sure it makes sense when it comes to gender and gender preferences!

# Indian Princess
narrative_event = {
	id = ko.410
	desc = EVTDESC_KO_179
	title = EVTTITLE_KO_179
	picture = GFX_evt_indian_bride

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_179
		hidden_tooltip = {
			narrative_event = { id = ko.411 }
		} 
	}
	option = {
		name = EVTOPTB_KO_179
		hidden_tooltip = {
			narrative_event = { id = ko.411	}
			random_child = {
				random_list = {
					50 = {
						add_character_modifier = {
							name = student_of_language	
							duration = 60
						} 
					}
					30 = {}
					20 = {
						add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
					}
				}
			}
		} 
	}
	option = { 
		name = EVTOPTC_KO_179
		trigger = { NOT = { dynasty = 1000078000 } } # If this is dynasty-dependent, at least one of the other options should probably be dynasty-dependent!
		hidden_tooltip = {
			narrative_event = { id = ko.411 }
		} 
	}
}


