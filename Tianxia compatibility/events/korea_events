# Burds
character_event = {
	id = ko.228
	desc = EVTDESC_KO_097
	picture = GFX_evt_feast

	is_triggered_only = yes	

	option = {
		name = EVTOPTA_KO_097
		trigger = {
			NOT = { trait = stressed }
			NOT = { trait = depressed }
			NOT = { trait = shy }
			NOT = { trait = wroth }
			NOT = { trait = craven }
			NOT = { trait = paranoid }
		}
		set_character_flag = bad_chuseok
		prestige = -10
	}
	option = {
		name = EVTOPTB_KO_097
		trigger = {
			trait = hunter
		}
		tooltip_info = hunter
		prestige = 30
		clr_character_flag = bad_chuseok
	}
	option = {
		name = EVTOPTC_KO_097
		tooltip_info = impaler
		trigger = {
			trait = impaler
			NOT = { trait = cruel }
		}
		add_character_modifier = {
			name = entertaining_entertainment
			days = 730
		}
		clr_character_flag = bad_chuseok
	}
	option = {
		name = EVTOPTC_KO_097
		tooltip_info = cruel
		trigger = {
			trait = cruel
			NOT = { trait = impaler }
		}
		add_character_modifier = {
			name = entertaining_entertainment
			days = 730
		}
		clr_character_flag = bad_chuseok
	}
	option = {
		name = EVTOPTD_KO_097
		tooltip_info = inbred
		trigger = {
			trait = inbred
			NOT = { trait = imbecile }
		}
		add_character_modifier = {
			name = good_times
			days = 730
		}
		clr_character_flag = bad_chuseok
	}
	option = {
		name = EVTOPTD_KO_097
		tooltip_info = imbecile
		trigger = {
			trait = imbecile
			NOT = { trait = inbred }
		}
		add_character_modifier = {
			name = good_times
			days = 730
		}
		clr_character_flag = bad_chuseok
	}
	option = {
		name = EVTOPTE_KO_097
		trigger = {
			OR = {
				trait = stressed
				trait = depressed
			}
		}
		if = {
			limit = { trait = stressed }
			remove_trait = stressed
		}
		if = {
			limit = { trait = depressed }
			remove_trait = depressed
		}
		add_character_modifier = {
			name = good_times
			days = 730
		}
		clr_character_flag = bad_chuseok
	}
	option = {
		name = EVTOPTF_KO_097
		trigger = {
			OR = {
				trait = shy
				trait = wroth
				trait = craven
				trait = paranoid
			}
		}
		add_trait = stressed
		set_character_flag = bad_chuseok
	}
}

