		add_trait = humble
	}
}

# Gaya introducing Soju
character_event = {
	id = ko.277
	desc = EVTDESC_KO_118.d
	picture = GFX_evt_bribe_declined
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		year = 1300 # Is it at all reasonable to believe that this event makes sense if you started well before 1300?
		NOT = {
			has_character_flag = soju_in_korea
		}
		has_landed_title = d_gaya
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = shy
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
	}
	
	option = {
		name = EVTOPTA_KO_118.d
		character_event = { id = ko.279 }	
		set_character_flag = soju_in_korea
	}
	
	option = {
		name = EVTOPTB_KO_118.d
		any_courtier = { # Not random?
			limit = {
				religion = sunni
			}
			opinion = {
				modifier = opinion_mutual_respect
				who = ROOT
				years = 8
			}
		}
	}
