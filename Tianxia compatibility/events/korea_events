# Killing my little pony
character_event = {
	id = ko.503
	
	desc = {
		picture = GFX_evt_magistrate
		text = EVTDESC_KO_503.1
		trigger = { is_female = yes } 
	}
	desc = {
		picture = GFX_evt_horse_friend
		text = EVTDESC_KO_503.2
		trigger = { is_female = no } 
	}

	is_triggered_only = yes
	
	# Isn't horsegfx Conclave-specific? If so, this needs to be locked to that DLC since the gfx will break otherwise...
	
	option = {
		name = EVTOPTA_KO_503
		character_event = { id = ko.504 }
		
		# This should probably be cut since creating a character just to kill them is more performance-intensive (and thus bad practice) than just having descriptive text...
		hidden_tooltip = {
			random_list = {
				50 = {
					create_character = {
						name = "Banchagbanchagkub"
						age = 16
						religion = ROOT
						culture = horse
						race = horse
						dynasty = NONE
						trait = horse
						trait = imbecile
						trait = stubborn
						trait = fair
						trait = strong
					}
				}
				50 = {
					create_character = {
						name = "Banchagbanchagkub"
						female = yes
						age = 16
						religion = ROOT
						culture = horse
						race = horse
						dynasty = NONE
						trait = horse
						trait = imbecile
						trait = stubborn
						trait = fair
						trait = strong
					}
				}
			}
		}
		new_character = {
			death = {
				death_reason = death_sacrificed_norse
				killer = ROOT
			}
		}	
	}
}

