	}

	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
