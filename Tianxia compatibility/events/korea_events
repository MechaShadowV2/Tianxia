	id = ko.974
	desc = EVTDESC_KO_582.3
	picture = GFX_evt_judged_by_ruler_ze_korean

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.3
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}	
	}
	option = {
		name = EVTOPTB_KO_582.3
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}	
		e_japan = { # There are no guarantees that e_japan has a holder!
			holder_scope = {
				character_event = { id = ko.975 days = 45 }
			}
		}			
	}
	option = {
		name = EVTOPTC_KO_582.3
		wealth = -120 # This is quite expensive for what you get...
		event_target:fishingvillage = {
			owner = {
				spawn_unit = {
					owner = ROOT
					province = ROOT
					home = ROOT
					troops = {
						archers = { 100 500 }
						light_infantry = { 100 200 }
					}
					attrition = 0.3
					maintenance_multiplier = 0.5
					reinforces = yes
					reinforce_rate_multiplier = 0.33
				}
			}
		}	
	}
	
	option = {
		name = EVTOPTD_KO_582.3
		wealth = -50
	}			
}

