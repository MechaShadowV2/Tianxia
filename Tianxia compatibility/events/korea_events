# The Story of Silk 9
narrative_event = {
	id = ko.740
	desc = EVTDESC_KO_412
	picture = GFX_evt_silk
	title = EVTTITLE_KO_412

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_412 # This option looks like it has no effect when you get it...
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_412
		any_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			opinion = {
				modifier = opinion_narrow_minded
				who = ROOT
				years = 1
			}	
		}
		hidden_tooltip = {
			random_child = {
				limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			add_trait = fair # Congenial traits are basically never added post-birth; should it really be added here?
			}
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 } 
			}
		}
	}
	option = {
		name = EVTOPTC_KO_412 # This has no real effect; why?
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTD_KO_412
		any_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			opinion = {
				modifier = opinion_open_minded
				who = ROOT
				years = 1
			}	
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}

# Daughter loves your marshal
character_event = {
	id = ko.741
	desc = EVTDESC_KO_413
	picture = GFX_evt_daughter_asking_father
	only_playable = yes

	is_triggered_only = yes
		
	trigger = {
		has_character_flag = daughter_marshal_kdrama # This flag can be set for non-adults and lacks proper attraction checks where it is set; fix this!
	}
	

	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = shy
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
	}
	
	option = {
		name = EVTOPTA_KO_413 # This has no real visible effect (the variables don't tell you anything)...
		
		# The variable changes probably should be hidden...
		change_variable = { which = "martial_suggestion" value = 2 }
		change_variable = { which = "their_luv" value = 1 }		
		
 	}
	option = {
		name = EVTOPTB_KO_413 # This has no real visible effect (the variables don't tell you anything)...
		
		# The variable changes probably should be hidden...
		change_variable = { which = "martial_suggestion" value = 1 }
		change_variable = { which = "their_luv" value = 1 }		
 	}		
	option = {
		name = EVTOPTC_KO_413 # This has no effect; why?
 	}
	option = {
		name = EVTOPTD_KO_413 # This has no real visible effect (the variables don't tell you anything)...
		
		# The variable changes probably should be hidden...
		change_variable = { which = "their_luv" value = 1 }
		random_list = {
			50 = {
				character_event = { id = ko.463 }
			}
			50 = {}
		}
	}
}
	
