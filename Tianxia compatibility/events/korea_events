# The Death of Wangbi U 4
narrative_event = {
	id = ko.731
	desc = EVTDESC_KO_403
	picture = GFX_evt_lunatic
	title = EVTTITLE_KO_403
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_403 # This doesn't really do anything...
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}

# The Story of Silk
