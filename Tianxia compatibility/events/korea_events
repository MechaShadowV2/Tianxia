# Why were the Hwarang so great
character_event = {
	id = ko.724

	picture = GFX_evt_war_planning
	border = GFX_event_normal_frame_war
	is_triggered_only = yes
	desc = EVTDESC_KO_345
	only_playable = yes
	capable_only = yes
	prisoner = no
	
	# Is this related to the other similar events?
	
	trigger = {
		has_focus = focus_war
		has_character_flag = martial_hagseng
		can_have_more_leadership_traits = yes
		OR = {
			NOT = { trait = inspiring_leader }
			NOT = { has_character_flag = strat_gosu }
			AND = {
				NOT = { trait = holy_warrior }
				trait = zealous
			}
		}
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 1.25
			trait = misguided_warrior
		}
		modifier = {
			factor = 1.5
			trait = tough_soldier
		}
		modifier = {
			factor = 2.0
			trait = skilled_tactician
		}
		modifier = {
			factor = 3.0
			trait = brilliant_strategist
		}
		modifier = {
			factor = 0.1
			NOT = { martial = 5 }
		}
		modifier = {
			factor = 0.5
			NOT = { martial = 8 }
		}
		modifier = {
			factor = 2.0
			NOT = { martial = 12 }
		}
		modifier = {
			factor = 2.0
			has_character_modifier = adept_of_strategy
		}
		modifier = {
			factor = 3.0
			trait = strategist
		}
		modifier = {
			factor = 0.25
			leader_traits = 1
		}
	}
	
	option = {
		name = EVTOPTA_KO_345 # Different color flowers
		character_event = { id = ko.725 }
	}
		option = {
		name = EVTOPTB_KO_345 # They owned land
	character_event = { id = ko.726 }
	}
		option = {
		name = EVTOPTC_KO_345 # Taekwondo
	character_event = { id = ko.727 }
	}
	
}


