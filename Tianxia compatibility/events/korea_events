# Mayor Jeolla
character_event = {
	id = ko.645
	desc = EVTDESC_KO_645
	picture = GFX_evt_scribe

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_645
		tooltip_info = gregarious
		trigger = { trait = gregarious } 
		d_jeolla = { # There is no check for Jeolla having a holder, or being remotely close to ROOT's realm, or the holder being a mayor!
			holder_scope = {
				opinion = {
					modifier = opinion_agreed_with_opinion
					who = ROOT
					years = 2
			    }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_645
		trigger = { NOT = { trait = gregarious } } 
		d_jeolla = { # There is no check for Jeolla having a holder, or being remotely close to ROOT's realm, or the holder being a mayor!
			holder_scope = {
				opinion = {
					modifier = opinion_disagreed_with_opinion
					who = ROOT
					years = 2
			    }
			}
		}
	} 		
	option = {
		name = EVTOPTC_KO_645
		trigger  = { learning = 10 }
		prestige = 20
	}
	option = {
		name = EVTOPTD_KO_645
		prestige = -10
	}
}

