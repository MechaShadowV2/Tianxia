		name = EVTOPTC_KO_523
		tooltip_info = learning
		trigger = { learning = 13 }
		prestige = 30
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = melanesian_pagan
			culture = andamanese
			female = no
			age = 40
			trait = dwarf
		}
	}
}

