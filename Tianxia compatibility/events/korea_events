# Birth of Jumong 4
narrative_event = {
	id = ko.400
	desc = EVTDESC_KO_169
	title = EVTTITLE_KO_169
	picture = GFX_evt_fishing

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_169
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_169
		any_consort = {
			random_list = {
				10 = {
					add_trait = cynical
				}
				20 = {
					add_trait = humble
				}
				20 = {
					opinion = {
						modifier = opinion_defended_me
						who = ROOT
						years = 20
					}
				}
				20 = {
					opinion = {
						modifier = opinion_unconvinced
						who = ROOT
						years = 20 
					}
				}
				40 = {}
			}
		} 	
		random_child = {
			limit = {
				is_female = yes
			}
			random_list = {
				20 = {
					add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
				}
				20 = {
					add_trait = idolizer  # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
				}
				20 = {
					add_trait = ambitious
				}
				20 = {
					remove_trait = zealous
				}
				20 = {
					opinion = {
						modifier = opinion_idiot
						who = ROOT
						years = 3
					}
				}
				40 = {}
			}
		} 	
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTC_KO_169
		random_dynasty_member = {
			random_list = {
				20 = {
					add_trait = cynical
				}
				20 = {
					add_trait = humble
				}
				20 = {
					add_trait = ambitious
				}
				20 = {
					remove_trait = zealous
				}
				20 = {
					opinion = {
						modifier = opinion_idiot
						who = ROOT
						years = 3
					}
				}
				40 = {}
			}
		} 	
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}		
}

