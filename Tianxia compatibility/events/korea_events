		if = {
			limit = {
				is_title_active = e_japan # Active titles can lack a holder, and ROOT could be the holder; fix this!
			} 
			e_japan = {
				holder_scope = {
					imprison = FROM # Not ROOT?
				}
			}
		}
	}
	option = {
		name = EVTOPTA_KO_633
		character_event = { id = ko.626 }
	}
}
	
