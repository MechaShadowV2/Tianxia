	id = ko.204
	desc = EVTDESC_KO_077
	
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_077
		opinion = {
			modifier = opinion_dislike
			who = FROM
			years = 2
		}
	}
}

# Is this copied from other places? If so, that's a problem without a DLC lock...

# Choose gift for spouse
character_event = {
	id = ko.205
	desc = EVTDESC_KO_078
	picture = GFX_evt_chuseok

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_078
		trigger = {
			spouse = { is_female = yes }
		}
		scaled_wealth = -0.2
		set_character_flag = chuseok_fine_gift_to_spouse
		hidden_tooltip = { character_event = { id = ko.207 days = 30 } }
	}
	option = {
		name = EVTOPTB_KO_078
		trigger = {
			spouse = { is_female = yes }
		}
		scaled_wealth = -0.05
		set_character_flag = chuseok_small_gift_to_spouse
		hidden_tooltip = { character_event = { id = ko.207 days = 30 } }
	}
	option = {
		name = EVTOPTC_KO_078
		trigger = {
			NOT = { trait = kind }
			spouse = { is_female = yes }
		}
		set_character_flag = chuseok_no_gift_to_spouse
		hidden_tooltip = { character_event = { id = ko.207 days = 30 } }
	}
	option = {
		name = EVTOPTD_KO_078
		trigger = {
			has_lover = yes
			spouse = {
				NOT = { is_lover = ROOT }
				is_female = yes
			}
		}
		hidden_tooltip = { character_event = { id = ko.206 } }
	}
	option = {
		name = EVTOPTE_KO_078
		trigger = {
			spouse = { is_female = no }
		}
		scaled_wealth = -0.2
		set_character_flag = chuseok_fine_gift_to_spouse
		hidden_tooltip = { character_event = { id = ko.207 days = 30 } }
	}
	option = {
		name = EVTOPTF_KO_078
		trigger = {
			spouse = { is_female = no }
		}
		scaled_wealth = -0.05
		set_character_flag = chuseok_small_gift_to_spouse
		hidden_tooltip = { character_event = { id = ko.207 days = 30 } }
	}
	option = {
		name = EVTOPTG_KO_078
		trigger = {
			NOT = { trait = kind }
			spouse = { is_female = no }
		}
		set_character_flag = chuseok_no_gift_to_spouse
		hidden_tooltip = { character_event = { id = ko.207 days = 30 } }
	}
	option = {
		name = EVTOPTH_KO_078
		trigger = {
			has_lover = yes
			spouse = {
				NOT = { is_lover = ROOT }
				is_female = no
			}
		}
		hidden_tooltip = { character_event = { id = ko.206 } }
	}
}

