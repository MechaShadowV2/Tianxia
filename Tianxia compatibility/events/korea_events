# Bidding
character_event = {
	id = ko.830

	is_triggered_only = yes

	hide_window = yes

	immediate = {
		random_list = {
			# A character with learning X+Y (Y>0) automatically has learning X, so these modifiers need work!
			# Why isn't steweardship checked?
			10 = {
				modifier = {
					factor = 12
					learning = 20 
				}
				FROM = { 
					character_event = { id = ko.818 }
				}
			}
			10 = {
				modifier = {
					factor = 11
					learning = 18 
				}
				FROM = { 
					character_event = { id = ko.819 }
				}
			}
			10 = {
				modifier = {
					factor = 10
					learning = 16 
				}
				FROM = { 
					character_event = { id = ko.820 }
				}
			}
			10 = {
				modifier = {
					factor = 9
					learning = 14 
				}
				FROM = { 
					character_event = { id = ko.821 }
				}
			}
			10 = {
				modifier = {
					factor = 8
