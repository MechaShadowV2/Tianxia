# Backstage
character_event = {
	id = ko.872

	is_triggered_only = yes

	hide_window = yes

	immediate = {
		random_list = {
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = -2 }
				}
				character_event = { id = ko.873 }
			}
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = -1 }
				}
				character_event = { id = ko.874 }
			} 
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 0 }
				} 
				character_event = { id = ko.875 }
			} 
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 1 }
				}
				character_event = { id = ko.876 }
			} 
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 2 }
				}
				character_event = { id = ko.877 }
			} 
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 3 }
				}
				character_event = { id = ko.878 }
			} 
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 4 }
				}
				character_event = { id = ko.879 }
			} 
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 5 }
				}
				character_event = { id = ko.880 }
			} 
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 6 }
				}
				character_event = { id = ko.881 }
			} 
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "circle_quality" value = 7 }
				}
				character_event = { id = ko.882 }
			}  
		}
		random_list = {
			10 = {
				modifier = {
					factor = 3
					has_character_flag = hired_mongolians
				}
				character_event = { id = ko.883 days = 1 }  #bad
			}
