character_event = {
	id = ko.911
	desc = EVTDESC_KO_578
	picture = GFX_evt_feast

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_578
		# There are no checks making sure that the same realm lord doesn't get five events!
		random_realm_lord = {
			limit = {
				has_character_flag = coming_to_mu_ritual
			}
			character_event = { id = ko.912 }
		}
		random_realm_lord = {
			limit = {
				has_character_flag = coming_to_mu_ritual
			}
			character_event = { id = ko.912 }
		}
		random_realm_lord = {
			limit = {
				has_character_flag = coming_to_mu_ritual
			}
			character_event = { id = ko.912 }
		}
		random_realm_lord = {
			limit = {
				has_character_flag = coming_to_mu_ritual
			}
			character_event = { id = ko.912 }
		}
		random_realm_lord = {
			limit = {
				has_character_flag = coming_to_mu_ritual
			}
			character_event = { id = ko.912 }
		}
		character_event = { id = ko.912 days = 1 } # Why is this delayed?				
	}
}

# You formulate your answer
character_event = {
	id = ko.912
	desc = EVTDESC_KO_578.1
	picture = GFX_evt_feast

	is_triggered_only = yes
	
	# FROM will get spammed with events here, as will any_realm_lord with the coming_to_mu_ritual flag; fix this!
	
	option = {
		name = EVTOPTA_KO_578.1
		random_list = {
			10 = {
				modifier = {
					factor = 100
						FROM = {
							trait = chaste
						} 
					}
					FROM = {
						character_event = { id = ko.913 }
					}
					any_realm_lord = { # Is this meant to be ROOT's realm lords or FROM's realm lords?
						limit = {
							has_character_flag = coming_to_mu_ritual
						}
						character_event = { id = ko.914 }
					}
				} 
				10 = {
					modifier = {
						factor = 100
						FROM = {
							trait = temperate
						}
					}
					FROM = {
						character_event = { id = ko.915 }
					}
					any_realm_lord = { # Is this meant to be ROOT's realm lords or FROM's realm lords?
						limit = {
							has_character_flag = coming_to_mu_ritual
						}
						character_event = { id = ko.916 }
					}
				} 
				10 = {
					modifier = {
						factor = 100
						FROM = {
							trait = charitable
						}
					}
					FROM = {
						character_event = { id = ko.917 }
					}
					any_realm_lord = { # Is this meant to be ROOT's realm lords or FROM's realm lords?
