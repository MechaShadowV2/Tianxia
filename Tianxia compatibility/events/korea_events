	id = ko.732
	desc = EVTDESC_KO_404
	title = EVTTITLE_KO_404
	picture = GFX_evt_daughter_asking_father

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_404
		hidden_tooltip = {
			narrative_event = { id = ko.733 }
			random_child = {
				limit = {
					is_female = yes
					is_alive = yes
					age = 7
					NOR = {
						age = 14
						trait = cynical
						trait = weak
					}
				}
				opinion = {
					modifier = tough_love_opinion
					who = ROOT
					years = 3
				}
				add_trait = humble # Why do you get a guaranteed chance of changing the trait of another character (who could be a human player)?		
			} 			 
		}			
	}
	
	option = {
		name = EVTOPTB_KO_404
		hidden_tooltip = {
			narrative_event = { id = ko.733 }
		}
	}
	
	option = {
		random_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 7
				NOR = {
					age = 14
					trait = cynical
					trait = weak
				}
			}
			opinion = {
				modifier = opinion_disgruntled
				who = ROOT
				years = 2
			}
			add_trait = humble # Why do you get a guaranteed chance of changing the trait of another character (who could be a human player)?	
		}
		job_chancellor = {
			opinion = {
				modifier = opinion_reasonable
				who = ROOT
				years = 2
			}
			add_trait = humble # Why do you get a guaranteed chance of changing the trait of another character (who could be a human player)?
		}
		name = EVTOPTC_KO_404
		hidden_tooltip = {
			narrative_event = { id = ko.733 }
		}
	}
