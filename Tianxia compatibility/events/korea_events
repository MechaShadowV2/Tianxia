# Make Buddhist statues
character_event = {
	id = ko.1068
	desc =  EVTDESC_KO_1008

	
	picture = GFX_evt_icon_eastern_group
	is_triggered_only = yes
	
	immediate = {
		any_dynasty_member_even_if_dead = {
			limit = {
				is_grandparent_of = ROOT
			}
			save_event_target_as = statuegrandparentamidist
		}
		any_dynasty_member_even_if_dead = {
			limit = {
				is_parent_of = ROOT
				is_female = no
			}
			save_event_target_as = statuegrandparentamidist
		}
		any_dynasty_member_even_if_dead = {
			limit = {
				is_parent_of = ROOT
				is_female = yes
			}
			save_event_target_as = statuegrandparentamidist
		}
	}
	
	# Why are three options the same?
	# Also, what happens if the event target is alive and intrigue < 10?
	
	option = {
		name = EVTOPTA_KO_1008
		trigger = { event_target:statuegrandparentamidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTA_KO_1008_TT }
		hidden_tooltip = {
			event_target:statuegrandparentamidist  = {
				clear_event_target = statuefatheramidist
			}
		}
		piety = 50
		scaled_wealth = -0.5	
	}

	option = {
		name = EVTOPTB_KO_1008
		trigger = { event_target:statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTB_KO_1008_TT }
		hidden_tooltip = {
			event_target:statuefatheramidist  = {
				clear_event_target = statuefatheramidist
			}
		}
		piety = 50
		scaled_wealth = -0.5		
	}
	option = {
		name = EVTOPTC_KO_1008
		trigger = { event_target:statuemotheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTB_KO_1008_TT }
		hidden_tooltip = {
			event_target:statuemotheramidist  = {
				clear_event_target = statuemotheramidist
			}
		}
		piety = 50	
		scaled_wealth = -0.5		
	}
	option = {
		name = EVTOPTD_KO_1008
		trigger = { intrigue = 10 }
		tooltip_info = intrigue
		custom_tooltip = { text = EVTOPTB_KO_1008_TT }	
	}		
}

# Jungyangjeol
character_event = {
	id = ko.1069
	desc =  EVTDESC_KO_1009

	trigger = {
		# If this is triggered on_yearly_pulse/etc., some characters will never be able to get this event!
		month = 9
		NOT = { month = 10 }
	} 
	picture = GFX_evt_mountain_shrine
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_1009
		any_courtier = {
			limit = {
				is_inaccessible_trigger = no 
				opinion = { who = ROOT value = 25 }  
				family_of_root_trigger = yes
			}
			# Nothing happens here; why?
		}
		# Are you meant to always get 2-3 events if you aren't injured/sick?
		hidden_tooltip = {
			if = {
				limit = {
					has_injury_trigger = yes
					has_disease_trigger = yes
				}
				character_event = { id = ko.1070 }
			}
		}
		hidden_tooltip = {
			if = {
				limit = {
					has_injury_trigger = no
					has_disease_trigger = no
				}
				character_event = { id = ko.1071 }
			}
		}
		hidden_tooltip = {
			if = {
				limit = {
					trait = slothful
				}
				character_event = { id = ko.1072 }
			}
		}
		custom_tooltip = { text = EVTOPTA_KO_1009_TT }
		piety = 50
		scaled_wealth = -0.1	
	}

	option = {
		name = EVTOPTB_KO_1009
		trigger = { event_target:korean_statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTB_KO_1009_TT }
		any_realm_character = {
			limit = {
				is_friend = ROOT
				opinion = {
					who = ROOT
					value = 90
				}
			}
			letter_event = { id = ko.1073 }
		} 
	}
		
	option = {
		name = EVTOPTC_KO_1009
		trigger = { event_target:korean_statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTB_KO_1009_TT }
		piety = 50
		scaled_wealth = -0.5		
	}
	option = {
		name = EVTOPTD_KO_1009 # This is the same as C
		trigger = { event_target:korean_statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTD_KO_1009_TT }
		piety = 50
		scaled_wealth = -0.5		
	}	
}

# Jungyangjeol Family member informed you are too sick to come
character_event = {
	id = ko.1070
	desc =  EVTDESC_KO_1010
	
	picture = GFX_evt_mountain_shrine
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_1010 # This has no effect; why?
		ai_chance = {
			factor = 3
			modifier = {
				factor = 4
				trait = content
			}				
			modifier = {
				factor = 4
				trait = depressed
			}
			modifier = {
				factor = 2
				trait = stressed
			}			
			modifier = {
				factor = 4
				trait = gluttonous
			}
			modifier = {
				factor = 4
				trait = feeble
			}			
		}
		custom_tooltip = { text = EVTOPTA_KO_1010_TT }	
	}
	option = {
		name = EVTOPTB_KO_1010

		custom_tooltip = { text = EVTOPTB_KO_1010_TT }
		# Are you meant to always get two events?
		hidden_tooltip = {
			if = {
				limit = {
					has_injury_trigger = yes
					has_disease_trigger = yes
				}
				character_event = { id = ko.1070 days = 365 }
			} 
			if = {
				limit = {
					has_injury_trigger = no
					has_disease_trigger = no
				}
				character_event = { id = ko.1071 days = 365 }
			}		
			any_courtier = {
				limit = {
					is_inaccessible_trigger = no 
					opinion = { who = ROOT value = 25 }
					family_of_root_trigger = yes
				}
				# Are you meant to always get two or three events?
				if = {
					limit = {
						has_injury_trigger = yes
						has_disease_trigger = yes
					}
					character_event = { id = ko.1070 days = 365 }
				}
				if = {
					limit = {
						has_injury_trigger = no
						has_disease_trigger = no
					}
					character_event = { id = ko.1071 days = 365 }
				} 
				if = {
					limit = {
						trait = slothful
					}
					character_event = { id = ko.1072 }
				} 
			}
		}
	}		
}

# Jungyangjeol Family member informed you are too fat to come
character_event = {
	id = ko.1072
	desc =  EVTDESC_KO_1012
	
	picture = GFX_evt_recovery_from_sickness
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_1012
		ai_chance = {
			factor = 3
			modifier = {
				factor = 4
				trait = content
			}				
			modifier = {
				factor = 4
				trait = depressed
			}
			modifier = {
				factor = 2
				trait = stressed
			}			
			modifier = {
				factor = 4
				trait = gluttonous
			}
			modifier = {
				factor = 4
				trait = feeble
			}			
		}
		custom_tooltip = { text = EVTOPTA_KO_1012_TT } # This has no effect; why?
	}
	option = {
		name = EVTOPTB_KO_1012

		custom_tooltip = { text = EVTOPTB_KO_1012_TT }
		hidden_tooltip = {
			random_list = {
				50 = {
					character_event = { id = ko.1073 }
				}
				50 = {
					character_event = { id = ko.1074 }
				}
			}
		}
	}
}	

