# Clear all character flags and modifiers
character_event = {
	id = ko.660

	hide_window = yes
	
	is_triggered_only = yes
	
	immediate = {
		remove_character_modifier = partaking_in_yongdeungje
		clr_character_flag = attending_yongdeungje
		clr_character_flag = do_not_disturb
		clr_character_flag = party_carousing
		set_character_flag = yongdeungje_begun
	}
}


