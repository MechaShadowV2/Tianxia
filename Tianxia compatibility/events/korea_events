# Wife or lover reacts to gift (or lack thereof)
character_event = {
	id = ko.207
	desc = EVTDESC_KO_080
	picture = GFX_evt_feast

	is_triggered_only = yes
	
	# Make sure that it is impossible to get more than four options!
	
	option = {
		name = EVTOPTA_KO_080
		trigger = {
			spouse = { is_female = yes }
			has_character_flag = chuseok_fine_gift_to_spouse
		}
		spouse = {
			opinion = {
				modifier = opinion_grateful
				who = ROOT
				years = 1
			}
			random = {
				chance = 30
				add_lover = ROOT
			}
		}
	}
	option = {
		name = EVTOPTB_KO_080
		trigger = {
			spouse = { is_female = yes }
			has_character_flag = chuseok_small_gift_to_spouse
		}
	}
	option = {
		name = EVTOPTC_KO_080
		trigger = {
			spouse = { is_female = yes }
			has_character_flag = chuseok_no_gift_to_spouse
		}
		spouse = {
			opinion = {
				modifier = opinion_insulted
				who = ROOT
				years = 1
			}
		}
	}
	option = { #d
		name = EVTOPTD_KO_080
		trigger = {
			any_lover = {
				is_female = yes
				NOT = {
					spouse = {
						character = ROOT
					}
				}
			}
			has_character_flag = chuseok_fine_gift_to_lover
			is_married = yes
		}
		random_lover = {
			limit = {
				is_female = yes
				NOT = {
					spouse = {
						character = ROOT
					}
				}
			}
		}
		spouse = {
			opinion = {
				modifier = opinion_furious
				who = ROOT
				years = 1
			}
		}
	}
