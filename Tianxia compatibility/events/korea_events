				modifier = {
					factor = 3
					has_character_flag = hired_hwarang
				}
				character_event = { id = ko.886 days = 1 }
			}
			10 = {
				modifier = {
					factor = 3
					has_character_flag = hired_locals
				}
				character_event = { id = ko.887 days = 1 }
			}
			10 = {
				modifier = {
					factor = 5
					has_character_flag = by_river
				}
				character_event = { id = ko.889 days = 1 }
			}
			10 = { 
				character_event = {  id = ko.884 days = 1 } #rabble
			}
			10 = { 
				character_event = {  id = ko.885 days = 1 } #stonethrowers
			}
			10 = {
				character_event = {  id = ko.888 days = 1 } #pigs
			}
