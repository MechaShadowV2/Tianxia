# Silver Bracelet locate
character_event = {
	id = ko.811
	desc = EVTDESC_KO_560.01
	picture = GFX_evt_gift

	is_triggered_only = yes
	
	immediate = {
		random_courtier  = {
			limit = {
				is_female = yes
			}
			save_event_target_as = female_k_friend		
		}
	}	
	
	option = {
		name = EVTOPTA_KO_560.01
		event_target:female_k_friend  = {
			character_event = { id = ko.812 }
			add_artifact = silver_bracelets
		}	
	}
	
	option = {
		name = EVTOPTB_KO_560.01 # I see no reason why you wouldn't go for this basically all the time since there isn't much of a drawback...
		add_trait = deceitful
		add_artifact = silver_bracelets
	}		
}

