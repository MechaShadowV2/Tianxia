character_event = {
	id = ko.034
	desc = EVTDESC_KO_003
	picture = GFX_evt_riders_forest_dusk

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_003
		custom_tooltip = { text = EVTOPTA_KO_003_TT }
		random_list = {
			50 = {
				character_event = { id = ko.037 }
			}
			50 = {
				character_event = { id = ko.038 }
			}
		}		
	}
	option = {
		name = EVTOPTB_KO_003
		custom_tooltip = { text = EVTOPTB_KO_003_TT }
		random_list = {
			34 = {
				character_event = { id = ko.039 }
			}
			33 = {
				character_event = { id = ko.040 }
			}
			33 = {
				character_event = { id = ko.041 }
			}
		}		
	}
	option = {
		name = EVTOPTC_KO_003
		tooltip_info = hunter
		trigger = { trait = hunter }

		random_list = {
			80 = {
				character_event = { id = ko.042 }
			}
			20 = {
				character_event = { id = ko.043 }
			}
		}
	}
}

