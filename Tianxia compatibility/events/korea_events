# Cups and balls - guess is always wrong
character_event = {
	id = ko.226
	desc = EVTTITLE_KO_093
	picture = GFX_evt_feast

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_095 # Oh snap
		prestige = -5
	}
	option = {
		name = EVTOPTB_KO_095 # Angry - I think you are cheating!
		tooltip_info = wroth
		trigger = {
			is_smart_trigger = no 
			trait = wroth
		}
		prestige = 15
		if = {
			limit = {
				NOT = { trait = cynical }
			}
			add_trait = cynical
		}
	}
	option = {
		name = EVTOPTC_KO_095 # I saw you cheating!
		tooltip_info = genius
		trigger = {
			trait = genius
			NOT = { 
				OR = {
				trait = quick
				trait = shrewd
				} 
			}
		}
		prestige = 30
	}
	option = {
		name = EVTOPTC_KO_095 # I saw you cheating!
		tooltip_info = quick
		trigger = {
			OR = {
				trait = quick
				trait = shrewd
			}
			NOT = { trait = genius }
		}
		prestige = 30
	}
}

