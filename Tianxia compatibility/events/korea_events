		name = EVTOPTB_KO_112 # This has no effect; why?
	}
}

# Kaya Farmers
character_event = {
	id = ko.246
	desc = EVTDESC_KO_113.a
	picture = GFX_evt_noble_haughty_talking_to_peasants
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		has_landed_title = c_jinju # Are we certain that this trigger is enough?
		# Also, vanilla has very few province-specific events; do we really need such events?
	}

	option = {
		name = EVTOPTA_KO_113.a
		scaled_wealth = -0.2
	}
	option = {
		name = EVTOPTB_KO_113.a
		random_list = {
			50 = {
				1774 = {
				add_province_modifier = {
					name = peasant_unrest
					duration = 1000
					}
				} 
			}
			50 = {}
		}
	}
	option = {
		name = EVTOPTC_KO_113.a
		tooltip_info = learning
		trigger = { learning = 10 }
		character_event = { id = ko.247 }
	}	
}
		
# Kaya Farmers 2
character_event = {
	id = ko.247
	desc = EVTDESC_KO_113.b
	picture = GFX_evt_noble_haughty_talking_to_peasants
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_113.b
		character_event = { id = ko.248 }
	}
	option = {
		name = EVTOPTB_KO_113.b # Why would you risk this option?
		random_list = {
			25 = { 
				1774 = {
					add_province_modifier = {
						name = peasant_unrest
