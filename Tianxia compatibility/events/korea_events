			limit = { is_female = yes }
			opinion = {
				modifier = opinion_enchanted
				who = ROOT
				years = 1
			}
		}			
	}
	option = {
		name = EVTOPTD_KO_389
		tooltip_info = homosexual
		trigger = { trait = homosexual  } # Are we sure that ROOT is male?
		job_marshal = {
			opinion = {
				modifier = opinion_was_praised
				who = ROOT
				years = 20
			}
		}		
	}	
}

