# Indian Princess 4
narrative_event = {
	id = ko.413
	desc = EVTDESC_KO_182
	title = EVTTITLE_KO_182
	picture = GFX_evt_indian_bride2

	is_triggered_only = yes

	immediate = {
		random_child = {
			limit = {
				is_alive = yes
				is_female = yes
				age = 7
				NOR = {
					age = 16
					trait = cynical
				}
			}
			character_event = { id = ko.426 days = 123 }		
		}
	}
	option = {
		name = EVTOPTA_KO_182
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
			random_child = {
				random_list = {
					50 = {
						add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
					}
					10 = {
						add_trait = lustful
					}
					20 = {
						add_trait = diligent
					}
					20 = {
						religion = buddhist # Random religion conversion? Shouldn't this be blocked by e.g. being reformed Muist/Zealous, etc.?
						prestige = -50
						piety = 25
					}
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_182
		any_courtier = { # Not random?
			limit = {
				is_adult = no
			}
			opinion = {
				modifier = opinion_impressed
				who = ROOT
				years = 5
			}
			add_trait = sympathy_indian # There probably needs to be a blocker for being Zealous here...
		}
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
		}
	}
	option = { 
		name = EVTOPTC_KO_182
		tooltip_info = learning
		trigger = { learning = 20 } # Extremely high!
		any_courtier = { # Not random?
			opinion = {
				modifier = opinion_impressed
				who = ROOT
				years = 3
			} 		
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}		
}
