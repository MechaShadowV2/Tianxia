narrative_event = {
	id = ko.403
	title = EVTTITLE_KO_172
	desc = EVTDESC_KO_172
	picture = GFX_evt_story_tiger

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_172
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
			random_child = {
				random_list = {
					20 = {
						add_trait = curious
					}
					20 = {
						add_trait = playful # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
					}
					5 = {
						add_trait = ambitious
					}
					20 = {
						remove_trait = craven
					}
					20 = {
						opinion = {
							modifier = opinion_idiot
							who = ROOT
							years = 3
						}
					}
					15 = {}
				}
			}
		}
	}
	
	option = {
		name = EVTOPTB_KO_172
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
		}
	}
	option = {
		name = EVTOPTC_KO_172 # Why are there two identical options?
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
