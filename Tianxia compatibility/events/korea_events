	id = ko.219
	desc = EVTDESC_KO_091
	picture = GFX_evt_kfeast

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_091
		random_realm_character = {
			limit = {
				has_character_flag = attending_chuseok_feast
				ROOT = { is_liege_of = THIS }
				is_female = no
			}
			tooltip = { prestige = 30 }
			ROOT = {
				opinion = {
					modifier = opinion_impressed
					who = PREV
					years = 5
				}
				any_realm_character = {
					limit = {
						has_character_flag = attending_chuseok_feast
						ROOT = { is_liege_of = THIS }
						NOT = { character = PREVPREV }
					}
					hidden_tooltip = {
						opinion = {
							modifier = opinion_impressed
							who = PREVPREV
							years = 5
						}
					}
				}
			}
			hidden_tooltip = { character_event = { id = ko.220 } }
		}
	}
}

