# Adku Beans
character_event = {
	id = ko.1055
	desc =  EVTDESC_KO_908.3

	picture = GFX_evt_small_town_sinic

	is_triggered_only = yes
	trigger = {
		# How is this triggered? If it is on_yearly_pulse/etc., not all characters will have the option to trigger it...
		month = 10
		NOT = {
			month = 11
		}
	}
	option = {
		name = EVTOPTA_KO_908.3
		custom_tooltip = { text = EVTOPTB_KO_908.3_TT }
		spouse = {
			opinion = {
				modifier = opinion_insulted
				who = ROOT
				years = 1
			}
		}
	}
	
	option = {
		name = EVTOPTB_KO_908.3
		custom_tooltip = { text = EVTOPTB_KO_908.3_TT }
		wealth = -2
		random_ward = {
			opinion = {
				modifier = opinion_grateful
				who = ROOT
				years = 1
			}
		}
	}
	option = {
		name = EVTOPTC_KO_908.3 # Why does this option have no effect?
		custom_tooltip = { text = EVTOPTC_KO_908.3_TT }
	}	
}

