			hidden_tooltip = {
				random_list = {
				50 = {
					top_liege = {
						character_event = { id = ko.305 days = 4 }
					}
					add_trait = kind
				} 
				50 = {
					piety = 20
				}
			}
		}
	}
	
	option = {
		name = EVTOPTB_KO_118.g
		set_global_flag = gaya_gone
		piety = 25	
	}
	
	option = {
		name = EVTOPTC_KO_118.g
		set_global_flag = gaya_gone
		wealth = 40	
		hidden_tooltip = { 	
			random_list = {
				20 = {} 
