# In royal tent night.
character_event = {
	id = ko.059
	desc = EVTDESC_KO_017
	picture = GFX_evt_story_tiger

	is_triggered_only = yes
	
	# These options MUST be updated!
	# The triggers don't make sense right now because you get the same outcome for the first two options AND have no visible option if intrigue < 6
	# There is also nothing preventing you from picking the low intrigue options (which presumably are worse) if you have high intrigue (e.g. C when you can pick D), which is weird
	option = {
		name = EVTOPTA_KO_017
		tooltip_info = intrigue
		trigger = { intrigue = 6 }
		character_event = { id = ko.063 }
	}
	option = {
		name = EVTOPTB_KO_017
		tooltip_info = intrigue
		trigger = { intrigue = 8 }
		character_event = { id = ko.063 }
	}
	option = {
		name = EVTOPTC_KO_017
		tooltip_info = intrigue
		trigger = { intrigue = 10 }
		random_list = {
			60 = {
				   character_event = { id = ko.062 }
		   }
			40 = { 
				   character_event = { id = ko.061 }
			}
		}
	}
	option = {
		name = EVTOPTD_KO_017
		custom_tooltip = { text = EVTOPTD_KO_017_TT }	
		tooltip_info = intrigue
		trigger = { intrigue = 14 }
		random_list = {
			30 = { 
				character_event = { id = ko.062 }			
			}
			70 = { 
				character_event = { id = ko.061 }			
			}
		}
	}	
}

# Act against Tiger at camp
character_event = {
	id = ko.061
	desc = EVTDESC_KO_036
	picture = GFX_evt_tribal_life
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_036
		prestige = 50
		random_list = {			
			50 = {
				add_trait = brave 
				hidden_tooltip = { character_event = { id = 38270 } }
			}
			50 = {}
		}
	}
	option = {
		name = EVTOPTB_KO_036
		tooltip_info = poet
		trigger = { trait = poet }
		prestige = 75
		random_list = {
			50 = {
				add_trait = brave
				character_event = { id = 38270 }
			}
			50 = {}		
		}
	}					
}

