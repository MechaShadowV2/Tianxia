# Stalemate
character_event = {
	id = ko.983
	desc = EVTDESC_KO_582.5
	picture = GFX_evt_battle

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.5
		prestige = -200
		random_demesne_province = {
			limit = {
				is_capital = yes # This makes random_province useless!
			}
			add_province_modifier = {
				name = missing_reinforcements
				duration = 200
			}
		}
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}
	}
	option = {
		name = EVTOPTB_KO_582.5
		random_demesne_province = {
			limit = {
				is_capital = yes # This makes random_province useless!
			}
			add_province_modifier = {
				name = missing_reinforcements
				duration = 20
			}
		}	
		event_target:fishingvillage = {
			destroy_random_building = yes # This has no effect; provinces have no buildings!
		} 
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_2 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		} 
		event_target:fishingvillage = {
			owner = { # Wasn't it a demesne province, making the owner ROOT?
				opinion = {
					modifier = opinion_furious
					who = ROOT
					years = 2
				}
			}
		}
		random_list = {
			50 = {
				character_event = { id = ko.984  }
			}
			50 = {
				character_event = { id = ko.985 }
			} 
		}			
	}
	option = {
		name = EVTOPTC_KO_582.5
		character_event = { id = ko.985 }
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}
		random_demesne_province = {
			limit = {
				is_capital = yes # This makes random_province useless!
			}
			add_province_modifier = {
				name = missing_reinforcements
				duration = 500
			}
		}			
	}			
}

