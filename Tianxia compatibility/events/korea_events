			change_martial = 1
		}
		job_marshal = {
				wealth = 20
				opinion = {
				modifier = opinion_grateful
				who = ROOT
				years = 1
			}			
		}			
 	}
	option = {
		name = EVTOPTB_KO_428
		event_target:infatuated_daughter  = {
			opinion = {
				modifier = opinion_outraged
				who = ROOT
				years = 1
			}		
		}
		job_marshal = {
			imprison = ROOT
		}	
	}
	option = {
		name = EVTOPTC_KO_428
		tooltip_info = intrigue
		trigger = { intrigue = 15 } 
		character_event = { id = ko.750 days = 1 }
	}
}
		
# Confront Marshal 4
character_event = {
	id = ko.749
	desc = EVTDESC_KO_419
	picture = GFX_evt_humiliation_prisoner

	is_triggered_only = yes
	
	trigger = {
		is_variable_equal = { which = "their_luv" value = 5 }
	}
	
	option = {
		name = EVTOPTA_KO_419
		wealth = -20
		event_target:infatuated_daughter  = {
			change_martial = 1
		}
		job_marshal = { # There are no checks for this being the same marshal since it could have changed since the first event; this probably also is the case for several more events before this...
			prisoner = no
			opinion = {
				modifier = opinion_unimpressed
				who = ROOT
				years = 1
			}			
		}			
 	}
	option = {
		name = EVTOPTB_KO_419
		job_marshal = { # There are no checks for this being the same marshal since it could have changed since the first event; this probably also is the case for several more events before this...
			prisoner = no
			opinion = {
				modifier = opinion_unimpressed
				who = ROOT
				years = 1
			}			
		}	
 	}
	option = {
		name = EVTOPTC_KO_419
		job_marshal = { # There are no checks for this being the same marshal since it could have changed since the first event; this probably also is the case for several more events before this...
			random_list = {
				33 = {
					add_trait = wounded
				}
				34 = {
					character_event = { id = ko.750 }
				}
				33 = {
					death = {
						death_reason = death_murder_unknown
						# Killer needs to be set, even if it officially is unknown!
					} 
				}
			}		
		}	
	}		
}

# Confront Marshal 5
character_event = {
	id = ko.750
	desc = EVTDESC_KO_420
	picture = GFX_evt_white_tiger

	is_triggered_only = yes
	
	trigger = {
		is_variable_equal = { which = "their_luv" value = 5 }
	}
	
	option = {
		name = EVTOPTA_KO_420
		clr_character_flag = daughter_marshal_kdrama
		
		# TODO: Grace penalty
		
		job_marshal = { # There is nothing guaranteeing that this is the initial marshal!
			death = {
				death_reason = death_execution_beheading
				# A killer needs to be set!
			}
		}			
		prestige = 50
		piety = -20		
		event_target:infatuated_daughter  = {
			add_trait = depressed	
		}		
 	}
	option = {
		name = EVTOPTB_KO_420
		event_target:infatuated_daughter  = {
			opinion = {
				modifier = opinion_my_saviour
				who = ROOT
				years = 40
			}		
		}				
		job_marshal = { # There is nothing guaranteeing that this is the initial marshal!
			add_lover = event_target:infatuated_daughter # Is the daughter guaranteed to be an adult here?
			# Attraction checks are also completely missing from this chain...
			opinion = {
				modifier = opinion_my_saviour
				who = ROOT
				years = 40
			}
		}	
	}
	option = {
		name = EVTOPTC_KO_420
		job_marshal = { # There is nothing guaranteeing that this is the initial marshal!
		opinion = {
			modifier = marshal_overruled
			who = ROOT
			years = 5
			}
		}	
	}
}

# Confront Daughter
character_event = {
	id = ko.751
	desc = EVTDESC_KO_422
	picture = GFX_evt_white_tiger

	is_triggered_only = yes
	
	trigger = {
		is_variable_equal = { which = "their_luv" value = 5 }
		has_character_flag = daughter_marshal_kdrama # This can be set for non-adult daughters and doesn't check attraction properly; fix this!
	}
	
	option = {
		name = EVTOPTA_KO_422
		clr_character_flag = daughter_marshal_kdrama
		job_marshal = { # There is nothing guaranteeing that this is the same marshal...
			add_spouse_matrilineal = event_target:infatuated_daughter # The marshal could be a player; forcing the player into a matrilinear marriage outside their dynasty would be bad?
			# There's also no check for the marshal not being married already...
		}			
		prestige = 50
		piety = -20		
		event_target:infatuated_daughter  = {
			reverse_add_favor = ROOT # Favours are Conclave-exclusive; this has no check for Conclave!
		}		
 	}
	option = {
		name = EVTOPTB_KO_422
		clr_character_flag = daughter_marshal_kdrama
		event_target:infatuated_daughter  = {
			opinion = {
				modifier = opinion_unhappy
				who = ROOT
				years = 40
			}		
		}				
		job_marshal = { # There is nothing guaranteeing that this is the same marshal...
			banish = yes
			opinion = {
				modifier = opinion_unhappy
				who = ROOT
				years = 40
			}
		}
	}
	option = {
		name = EVTOPTC_KO_422
		clr_character_flag = daughter_marshal_kdrama
		event_target:infatuated_daughter  = {
			add_trait = nun # Are there any guarantees that the daughter is of a religion that has nuns?
		}
	}
}	

# Marshal suggests marriage
character_event = {
	id = ko.752
	desc = EVTDESC_KO_423
	picture = GFX_evt_white_tiger

	is_triggered_only = yes

	trigger = {
		is_variable_equal = { which = "martial_suggestion" value = 5 } 
		has_character_flag = daughter_marshal_kdrama # This can be set for non-adult daughters and doesn't check attraction properly; fix this!
	}

	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = shy
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
	}
	
	option = {
		name = EVTOPTA_KO_423
		event_target:infatuated_daughter  = {
			opinion = {
				modifier = opinion_grateful
				who = ROOT
				years = 5
			}		
		}			
		job_marshal = { # There is nothing guaranteeing that this is the same marshal, or that the marshal is unmarried...
			opinion = {
				modifier = opinion_promised_marriage
				who = ROOT
				years = 1
			}
		}	
	}
	option = {
		name = EVTOPTB_KO_423 # This has no visible effect; why?
		set_character_flag = promised_prestige_marriage
 	}
	option = {
		name = EVTOPTC_KO_423
		clr_character_flag = daughter_marshal_kdrama		
		job_marshal = { # There is nothing guaranteeing that this is the same marshal
			opinion = {
				modifier = opinion_denied_marriage
				who = ROOT
				years = 20
			}
		}
	}
}

# Marshal suggests marriage prestige
character_event = {
	id = ko.753
	desc = EVTDESC_KO_425
	picture = GFX_evt_white_tiger

	is_triggered_only = yes

	trigger = {		
		has_character_flag = promised_prestige_marriage
		has_character_flag = daughter_marshal_kdrama # This can be set for non-adult daughters and doesn't check attraction properly; fix this!
		job_marshal = {	prestige = 1000	} # There is nothing guaranteeing that this is the same marshal, or that the marshal is unmarried...
	}

	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = shy
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
	}
	
	option = {
		clr_character_flag = daughter_marshal_kdrama
		name = EVTOPTA_KO_425
		event_target:infatuated_daughter  = {
			opinion = {
			modifier = opinion_disappointed
			who = ROOT
			years = 5
			}		
		}			
		job_marshal = { # There is nothing guaranteeing that this is the same marshal
			opinion = {
				modifier = not_supportive_liege
				who = ROOT
				years = 5
			}
		}	
	}
		
	option = {
		name = EVTOPTB_KO_425
		job_marshal = { # There is nothing guaranteeing that this is the same marshal, or that the marshal is unmarried...
			add_spouse_matrilineal = event_target:infatuated_daughter # The marshal could be a player; forcing the player into a matrilinear marriage would be bad!
		}			
		event_target:infatuated_daughter  = {
			reverse_add_favor = ROOT # Favours are Conclave-exclusive; where's the check for Conclave?
		}		
 	}			
}

