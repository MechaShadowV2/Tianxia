	desc = EVTDESC_KO_582.1
	picture = GFX_evt_judged_by_ruler_ze_korean

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.1 # The character creation here is pointless; just kill the "character" without creating them!
  		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = no
			age = 65
			trait = craven
			trait = fair
			trait = quick
			attributes = {
				stewardship = 10
			}
		}
		new_character = {
			death = {
				death_reason = death_execution_beheading
				killer = ROOT
			}
		}			
	}
	option = {
		name = EVTOPTB_KO_582.1
  		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = no
			age = 36
			trait = cruel
			trait = quick
			attributes = {
				martial = 10
			}
		}
		new_character = {
			imprison = ROOT
		}
	}					
}

