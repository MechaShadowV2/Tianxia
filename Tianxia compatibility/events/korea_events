# Reform of paganism
character_event = {
	id = ko.661
	desc = EVTDESC_KO_356
	picture = GFX_evt_religious_exultation

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_356
		prestige = 100
	}
	option = {
		name = EVTOPTB_KO_356
		prestige = -200
		piety = 200
		character_event = { id = ko.661 days = 10 random = 6 }
	}
	option = {
		name = EVTOPTC_KO_356
		wealth = -50
		piety = 50
		random_demesne_province = {
			limit = {
				is_capital = yes # Why check for a random province and then limit it to the capital?
			}
			add_province_modifier = {
				name = new_shrines
				duration = -1 # Why do Muists get modifiers that last forever?
			}
			add_province_modifier = {
				name = blessed_lands
				duration = -1 # Why do Muists get modifiers that last forever?
			}
		}
	}
}

