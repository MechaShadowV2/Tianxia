# Wine Festival
character_event = {
	id = ko.556
	desc = EVTDESC_KO_534
	picture = GFX_evt_market

	is_triggered_only = yes

	immediate = {
		random_independent_ruler = { # This event should probably have a trigger checking that there is a possible target...
			limit = {
				controls_religion = no
				is_adult = yes
				mercenary = no
				holy_order = no
				trait = gardener
				OR = { 
					culture_group = koreanic
					culture_group = japonic
					culture = han # Not chinese_group?
				}
				is_within_diplo_range = ROOT
				NOR = {
					trait = incapable
					prisoner = yes
					ROOT = {
						any_liege = {
							character = PREVPREV
						}
					}
				}
			}
			character_event = { id = ko.557 } # Is this the right event?
		}
	}	
	
	option = { 
		name = EVTOPTA_KO_534
		add_character_modifier = {
			name = wine_meat
			duration = 40
		}		
	}
		
	option = { 
		name = EVTOPTB_KO_534 # This has no effect; why?
	}
		
	option = { 
		name = EVTOPTC_KO_534
		random_list = {
			33 = {
				character_event = { id = ko.559 }
			} 
			33 = {
				character_event = { id = ko.560 }
			}
			34 = {
				character_event = { id = ko.561 }
			}
		}
	}		
}

# You win
character_event = {
	id = ko.557
	desc = EVTDESC_KO_536.1
	picture = GFX_evt_Banquet_hall_Feast

	is_triggered_only = yes

	option = { 
		FROM = {
			character_event = { id = ko.558 }
		}	
		name = EVTOPTA_KO_536.1 # Why is this option available if you're Greedy?
		add_character_modifier = {
			name = wine_meat
			duration = 365
		}
	}
	option = { 
		name = EVTOPTB_KO_536.1
		tooltip_info = greedy
		FROM = {
			character_event = { id = ko.558 }
		}	
		trigger = { trait = greedy }
		scaled_wealth = 0.5 # Does this money spawn out of thin air and regardless of FROM's wealth?
		add_character_modifier = {
			name = wine_meat
			duration = 365
		}	
	}		
}

