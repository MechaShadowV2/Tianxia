# Mongols introducing Soju
character_event = {
	id = ko.278
	desc = EVTDESC_KO_118.d
	picture = GFX_evt_bribe_declined
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		is_tributary = yes # Whose tributary? You could be a tributary of the Aztecs and have this evaluate true!
		year = 1300 # Is it at all reasonable to believe that this event makes sense if you started well before 1300?
		NOT = {
			has_character_flag = soju_in_korea
		}
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
