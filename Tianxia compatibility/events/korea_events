# Hwajeonnori Bake?
character_event = {
	id = ko.804
	desc = EVTDESC_KO_557.1
	picture = GFX_evt_flowerpancakes

	is_triggered_only = yes

	
	option = {
		name = EVTOPTA_KO_557.1
		hidden_tooltip = {
			random_list = {
				33 = {
					modifier = {
						factor = 3 
						learning = 12 # A character with 12 learning automatically has 8 and 4 learning too
					}
					character_event = { id = ko.805 }
				}
				33 = {
					modifier = {
						factor = 3
						learning = 8 # A character with 12 learning automatically has 4 learning too
					}
					character_event = { id = ko.806 } 
				}
				33 = {
					modifier = {
						factor = 3
						learning = 4 # Very few characters have below 4 in any stat
					}
					character_event = { id = ko.807 }
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_557.1
		character_event = { id = ko.808 days = 1 } 
	}
}	

