# Fishing Village Cleanup
character_event = {
	id = ko.1022

	is_triggered_only = yes
	hide_window = yes

	immediate = {
		event_target:fishingvillage = {
			clear_event_target = fishingvillage # You probably want to clear this for ROOT instead...
		}
		set_variable = { which = port_prosperity value = 0 }	
	}
}
	
# The Tale of Momju
narrative_event = {
	id = ko.1023
	desc = EVTDESC_KO_115
	title = EVTTITLE_KO_700
	picture = GFX_evt_holy_tree

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_700
		hidden_tooltip = {
			narrative_event = { id = ko.1024 }
		}		
	}
	
	option = {
		name = EVTOPTB_KO_700 # Why would you pick A or C when this is better?
		hidden_tooltip = {
			narrative_event = { id = ko.1024 }
		}
		random_child = {
			limit = {
				is_alive = yes
				age = 7
				NOR = {
					age = 16
					trait = cynical
				}
			}
			opinion = {
				modifier = opinion_agree
				who = ROOT
				years = 2
			}		
		}		
	}
	
	option = {
		name = EVTOPTC_KO_700 # This is the same as A
		hidden_tooltip = {
			narrative_event = { id = ko.1024 }
		}			
	}
}


# The Tale of Momju 2
narrative_event = {
	id = ko.1024
	desc = EVTDESC_KO_701
	title = EVTTITLE_KO_701
	picture = GFX_evt_mu_rattle

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_701
		hidden_tooltip = {
			narrative_event = { id = ko.1025 }
		}
		random_child = {
				limit = {
				is_alive = yes
				age = 7
				NOR = {
					age = 16
					trait = cynical
				}
			}
			add_trait = cynical	
		}
	}
	option = {
		name = EVTOPTB_KO_701
		piety = 1 # This is a pointless reward
		hidden_tooltip = {
			narrative_event = { id = ko.1025 }
		}
	}
	option = {
		name = EVTOPTC_KO_701 # Why would you ever pick this option?
		hidden_tooltip = {
			narrative_event = { id = ko.1025 }
		}
	}
}


# The Tale of Momju 3
narrative_event = {
	id = ko.1025
	desc = EVTDESC_KO_702
	title = EVTTITLE_KO_702
	picture = GFX_evt_monk_eastern_group

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_702
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_702
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
				random_child = {
					limit = {
						is_female = yes
						is_alive = yes
						age = 6
						NOR = {
							age = 16
							trait = cynical
							trait = weak
						}
					}
					change_intrigue = 1
				}
			}
		}
	}	
	option = {
		name = EVTOPTC_KO_702
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
		random_child = {
			limit = {
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			add_trait = curious	 # This is a Conclave childhood trait, but there is no check for Conclave!
		}			
	}
	option = {
		name = EVTOPTD_KO_702
		trigger = { has_dlc = "Reapers" }
		hidden_tooltip = {
			character_event = { id = RIP.21000 days = 30 }
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}			
	}	
}

