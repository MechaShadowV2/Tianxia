		name = EVTOPTA_KO_560.11
		ai_chance = {
			factor = 2
			modifier  = {
				factor = 5
				opinion = { who = from value = 30 }
			}
			modifier  = {
				factor = 2
				opinion = { who = from value = 15 }
			}
			modifier  = {
				factor = 2
				trait = humble
			}
		}			
		prestige = -100
		opinion = {
			modifier = opinion_disgruntled
			who = FROM
			years = 2
		}
		FROM = {
			character_event = { id = ko.837 }
		}
	}
	
	option = {
		name = EVTOPTB_KO_560.11
		ai_chance = {
			factor = 2
			modifier  = {
				factor = 5
				opinion = { who = from value = -30 }
			}
			modifier  = {
				factor = 2
				opinion = { who = from value = -15 }
			}
			modifier  = {
				factor = 2
				trait = wroth
			}
			modifier  = {
				factor = 2
				trait = ambitious
			}
			modifier  = {
				factor = 2
				trait = arbitrary
			}
			modifier  = {
				factor = 3
				primary_title = { higher_tier_than = FROM } # Isn't this impossible since ROOT is Lowborn and landed characters automatically get a dynasty?
			}			
		}			
		FROM = { 			
			opinion = {
				modifier = opinion_unappreciated
				who = ROOT
				years = 1
			}
		}
		FROM = {
			character_event = { id = ko.838 }
		}
	}
}

