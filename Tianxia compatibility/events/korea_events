	desc = EVTDESC_KO_038
	picture = GFX_evt_white_tiger_happy

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_038
		job_marshal = {
			prestige = -10
			opinion = {
				modifier = opinion_disappointed
				who = ROOT
				years = 3
			}
			random_list = {
				83 = {}
				10 = {
					change_martial = 1
				}
				7 = {
					add_trait = wroth
					hidden_tooltip = { character_event = { id = 38254 } }
				}
			}
		}
	}
}		
	
