			15 = { character_event = { id = ko.284 days = 2 } } 
			15 = { character_event = { id = ko.285 days = 2 } }
			15 = { character_event = { id = ko.286 days = 2 } } 
			15 = { character_event = { id = ko.287 days = 2 } }
		}	
