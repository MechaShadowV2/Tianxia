# Tending graves
character_event = {
	id = ko.237
	desc = EVTDESC_KO_107
	picture = GFX_evt_graveyard
	only_playable = yes
	is_triggered_only = yes
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 0.5
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
	}
	
	option = {
		name = EVTOPTA_KO_107
		tooltip_info =  stewardship
		trigger = { stewardship = 15 }
		prestige = 30
		wealth = -5
		add_character_modifier = {
			name = spiritual_increase_piety	
			duration = 200
		}
	}	
	option = {
		name = EVTOPTB_KO_107
		wealth = -20
		prestige = 30
		add_character_modifier = {
			name = spiritual_increase_piety
			duration = 200
		}
	}
	option = {
		name = EVTOPTC_KO_107
		random_list = {
			20 = {
				add_character_modifier = {
					name = despised_aristocrat	
					duration = 60
				} 
			}
			80 = {}
		}
	}
	option = {
		name = EVTOPTD_KO_107
		tooltip_info = gardener
		trigger = { trait = gardener }
		random_list = {
			20 = {
				add_character_modifier = {
					name = despised_aristocrat	
					duration = 60
				}
			}
			80 = {}
		}
		random_demesne_province = {
			limit = {
				is_capital = yes
			}
			add_province_modifier = {
				name = ancestral_monument
				duration = 270
			}
		}	
		add_character_modifier = {
			name = the_gardener	
			duration = 200
		}
	}
}

# Giving drink to kids
character_event = {
	id = ko.238
	desc = EVTDESC_KO_105.1
	desc = EVTDESC_KO_105.2
	picture = GFX_evt_quarrel
	only_playable = yes
	
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_105
		spouse = {
