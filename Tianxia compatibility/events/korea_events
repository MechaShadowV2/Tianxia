# Acuse The Mayor
character_event = {
	id = ko.075
	desc = EVTDESC_KO_045
	picture = GFX_evt_magistrate
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_045
		create_character = { # Wouldn't it be better to pick an existing mayor?
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = no
			age = 65
			trait = craven
			trait = fair
			trait = quick
			attributes = {
			stewardship = 10
			}
		}
		new_character = {
			imprison = ROOT
		}
	}
	option = {
		name = EVTOPTB_KO_045
		random_list = {
			20 = {}
			20 = {
				change_intrigue = 1
			}
			20 = {
				add_trait = poet
			}
			20 = {
				add_trait = deceitful
				hidden_tooltip = { character_event = { id = 38255 } }
			}
			20 = {
				add_trait = arbitrary
				hidden_tooltip = { character_event = { id = 38166 } }
			}
		}
	}
	option = {
		name = EVTOPTC_KO_045
  		character_event = { id = ko.059 }
	}
}
	
