	id = ko.549
	desc = EVTDESC_KO_547
	picture = GFX_evt_lovers
	
	# This lacks valid attraction checks, checks for being inaccessible, checks for the dance partner potentially being chaste/etc. and thus unlikely to tumble, opinion checks, and various other things
	# Also, *all* vanilla non-homosexual tumbling events come with a pregnancy (and STD) risk (assuming the woman can get pregnant and the man isn't a eunuch), so for consistency that should be here too!

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_547
		remove_trait = stressed
	}
}

