# Tiger climbing walls
character_event = {
	id = ko.050
	desc = EVTDESC_KO_014
	picture = GFX_evt_nighttown_sinic

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_014
		random_list = {
			50 = {
				character_event = { id = ko.051 }
			}
			50 = {
				character_event = { id = ko.052 }
			}
		}		
	}
	option = {
		name = EVTOPTB_KO_014

		trigger = {
			NOT = { trait = wounded }
		}
		if = {
			limit = {
				OR = {
					trait = organizer
					trait = experimenter
				}
			}
			random_list = {
				80 = { character_event = { id = ko.055 } } 
				20 = { character_event = { id = ko.056 } } 
			}
		}
		if = {
			limit = {
				NOT = { trait = organizer }
				NOT = { trait = experimenter }
			}
			random_list = {
				50 = { character_event = { id = ko.055 } } 
				50 = { character_event = { id = ko.056 } }
			}
		}
	}
	option = {
		name = EVTOPTC_KO_014
		tooltip_info = wroth
		trigger = { trait = wroth }
		character_event = { id = ko.048 }
	}
}

