# Oldest Child gives sacrifice
character_event = {
	id = ko.602
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_602.1
		trigger = { 
			num_of_children = 0 
		}
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_602.2
		trigger = {
			capital_scope = { # Why check it like this?
				owner = {
					num_of_children = 1	
					any_child  = {
						is_female = yes
					}
				}
			}
		} 
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_602.3
		trigger = { 
			capital_scope = { # Why check it like this?
				owner = {
					num_of_children = 1
					any_child  = {
						is_female = no
						is_adult = no
					}
				}
			}
		}
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_602.4
		trigger = {
			capital_scope = { # Why check it like this?
				owner = {		
				num_of_children = 1
				any_child  = {					
						is_female = no
						is_adult = yes
					}
				} 
			}
		}
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_602.5
		trigger = { 
			capital_scope = { # Why check it like this?
				owner = {
					any_child  = {
						is_female = no
						is_adult = yes
						is_married = yes
					}
				} 
			}
		}
	}

	is_triggered_only = yes	
	
	option = {
		name = EVTOPTA_KO_602
		character_event = { id = ko.603 }
	}
}

# Second Child gives sacrifice
character_event = {
	id = ko.603
	
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_603.1
		trigger = {	
			any_child  = {
				is_female = no
				is_adult = no
			}
			any_sibling = {
				is_older_than = ROOT
				is_female = no
				is_adult = yes
			}
		}
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_603.2
		trigger = {	
			any_child  = {
				is_female = no
				is_adult = no
			}
			any_sibling = {
				NOT = { is_older_than = ROOT }
				is_female = no
				is_adult = yes
			}
		}
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_603.3
		trigger = {
			any_child  = {
				is_female = no
				is_adult = yes
				is_pretender = yes	
			}
		} 
	}
	desc = {
		picture = GFX_evt_shaman
		text = EVTDESC_KO_603.2
		trigger = {
			num_of_children = 0 
			any_sibling = { 
				is_female = no
				is_alive = no	
			}
		}
	}
	
	is_triggered_only = yes
	
	option = {
		character_event = { id = ko.605 }
		name = EVTOPTA_KO_603
	}
}

		
