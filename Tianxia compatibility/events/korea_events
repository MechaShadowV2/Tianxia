	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_002
		custom_tooltip = { text = EVTOPTA_KO_002_TT }	
		 character_event = { id = ko.034 }								
	}
	option = {
		name = EVTOPTB_KO_002
		custom_tooltip = { text = EVTOPTB_KO_002_TT }	
		 character_event = { id = ko.032 }								
	}
	option = {
	tooltip_info = poet
	trigger = { trait = poet }
		name = EVTOPTC_KO_002
									
	}
}

