# Run down village
character_event = {
	id = ko.994
	desc = EVTDESC_KO_583
	picture = GFX_evt_fishing_village_one

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_583
		set_variable = { which = "port_prosperity" value = 1 }
		
		add_character_modifier = {
			name = port_of_calling
			duration = 4000
		}
		hidden_tooltip = {
			character_event = { id = ko.995 days = 40 }
			character_event = { id = ko.1016 days = 4000 }								
			if = { #Poem
				limit = {
					trait = envious
					diplomacy = 8
					trait = paranoid
					trait = poet
					trait = gregarious
				}
			random = {
				chance = 30
				character_event = { id = ko.996 days = 40 random = 365 } }
			}
			if = { #Fishnet
			limit = {
				is_married = yes
				is_female = no
				war = no
			}
			random = {
				chance = 30
				character_event = { id = ko.997 days = 40 random = 365 } }
			}
			if = { #Army spending
			limit = {
				capable_only = yes
				prisoner = no
			}
			random = {
				chance = 30
				character_event = { id = ko.999 days = 40 random = 365 } }
			}
			if = { #Complaining merchant
			limit = {
				capable_only = yes
				prisoner = no
			}
			random = {
				chance = 30
				character_event = { id = ko.1002 days = 40 random = 365 } }
			}
			if = { #Port infastructure
			limit = {
				capable_only = yes
				prisoner = no
			}
			random = {
				chance = 30
				character_event = { id = ko.1012 days = 40 random = 365 } }
			}			
		}
	}
	option = {
		name = EVTOPTB_KO_583 # This does nothing; why?
	}
}

# Options to improve port
character_event = {
	id = ko.995
	desc = EVTDESC_KO_583.1
	picture = GFX_evt_fishing_village_one

	is_triggered_only = yes

	
	option = {
		name = EVTOPTA_KO_583.1
		wealth = -40
		change_variable = { which = "port_prosperity" value = 4 }
		character_event = { id = ko.993 }
	}
	option = {
		name = EVTOPTB_KO_583.1
		change_variable = { which = "port_prosperity" value = 2 }
		event_target:fishingvillage = {
			owner = { # Isn't this ROOT since it is a demesne province?
				opinion = {
					modifier = opinion_mad_as_hell
					who = ROOT
					years = 3
				}	 
			} 
		}
	}
	option = {
		name = EVTOPTC_KO_583.1
		hidden_tooltip = { 
			if = { #Piracy
				limit = {
					capable_only = yes
					prisoner = no
				}
				random = {
					chance = 30
					character_event = { id = ko.1010 days = 10 random = 365 }
				}
			}
			if = { #Smuggling
			limit = {
				capable_only = yes
				prisoner = no
			}
			random = {
				chance = 30
				character_event = { id = ko.1011 days = 10 random = 365 }
			}
		}
	}
	change_variable = { which = "port_prosperity" value = 6 }
	event_target:fishingvillage = {
		add_province_modifier = {
			name = trade_port # Isn't this a RD modifier?
			days = 2000
			} 			
		}
	}	
}

# Court hates the fishing village
character_event = {
	id = ko.996
	desc = EVTDESC_KO_583.2
	picture = GFX_evt_courtiers

	is_triggered_only = yes
	trigger = {
		has_character_modifier = port_of_calling
	}
	
	option = {
		name = EVTOPTA_KO_583.2
		add_character_modifier = {
			name = vicious_rumours
			duration = 365
		}
		random_list = {
			20 = {
				add_trait = stressed
			}
			20 = {
				change_variable = { which = "port_prosperity" value = 1 }
			} 
		}			
	}
	option = {
		name = EVTOPTB_KO_583.2
		job_treasurer = {
			random_list = {
				20 = {
					add_trait = stressed
				}
				# Why are you changing the value for the steward and not ROOT?
				20 = {
					modifier = {
						factor = 3
						stewardship = 8
					}
					change_variable = { which = "port_prosperity" value = -1 } # Why does this (presumably) bad outcome happen more often with higher stewardship
				} 
				20 = {
					modifier = {
						factor = 3
						stewardship = 6
					}
					change_variable = { which = "port_prosperity" value = 1 }
				}
				20 = {
					modifier = {
						factor = 3
						stewardship = 5
					}
					change_variable = { which = "port_prosperity" value = 2 }
				}
				20 = {
					modifier = {
						factor = 3
						stewardship = 5
					}
					change_variable = { which = "port_prosperity" value = 3 }
				}
				20 = {
					modifier = {
						factor = 3
						stewardship = 5
					}
					change_variable = { which = "port_prosperity" value = 4 }
				} 	
			}
		}
	}
	option = {
		name = EVTOPTC_KO_583.2
		hidden_effect = {
			remove_character_modifier = port_of_calling
			character_event = { id = ko.1022 }
			event_target:fishingvillage = {
				remove_province_modifier = trade_port
			}
		}  			
	}		
}

# Fishing Nets
character_event = {
	id = ko.997
	desc = EVTDESC_KO_583.3
	picture = GFX_evt_family

	is_triggered_only = yes
	trigger = {
		has_character_modifier = port_of_calling
	}
	
	option = {
		name = EVTOPTA_KO_583.3
		spouse = {
			opinion = {
				modifier = glad
				who = ROOT
				years = 3
			}
		}
	}
	option = {
		name = EVTOPTB_KO_583.3
		tooltip_info = diplomacy
		trigger = { diplomacy = 10 }
		spouse = {
			opinion = {
				modifier = flattered
				who = ROOT
				years = 3
			}
		}
	}
	option = {
		name = EVTOPTC_KO_583.3
		tooltip_info = learning
		trigger = { learning = 10 }
		character_event = { id = ko.998 }
	}
	option = {
		name = EVTOPTD_KO_583.3 # Why would you pick this option when it only has negative consequences?
		spouse = {
			opinion = {
				modifier = opinion_mad_as_hell
				who = ROOT
				years = 8
			}
		}
		trigger = {
			spouse = {
				trait = ugly
			}
		}
	}
}
			
