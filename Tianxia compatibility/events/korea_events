		}
	}
	
	option = {
		name = EVTOPTA_KO_401
		random_child = {
			limit = {
				is_female = no
				is_alive = yes
				age = 12
				NOR = {
					age = 15
					trait = cynical
				}
			}
			add_trait = lustful	
		}
		hidden_tooltip = {
			narrative_event = { id = ko.730 }
		}
	}
	option = {
		name = EVTOPTB_KO_401
		random_child = {					
			limit = {
				is_female = no
				is_alive = yes
				age = 7
				NOR = {
					age = 15
					trait = cynical
				}
			}
			add_friend = event_target:brotherlyfriend	
		}
		hidden_tooltip = {
			narrative_event = { id = ko.730 }
		}
	}
	option = {
		name = EVTOPTC_KO_401
		hidden_tooltip = {
