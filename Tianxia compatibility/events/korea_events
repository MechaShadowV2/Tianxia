	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_571.3
		hidden_tooltip = {			
			random_list = {
				90 = {
					change_variable = { which = "circle_quality" value = 1 }
				}
				10 = {
					modifier = {
						factor = 30
						trait = slothful
					}
					change_variable = { which = "circle_quality" value = -1 }
				}
			}
			character_event = { id = ko.872 days = 2 }			
		}
	}
	option = {
		name = EVTOPTB_KO_571.3
		tooltip_info = stewardship
		trigger = { stewardship = 10 }
		hidden_tooltip = {
			character_event = { id = ko.880 days = 6 random = 2 }	
			change_variable = { which = "circle_quality" value = 2 }
			liege = {
				job_marshal = {
					character_event = { id = ko.870 days = 1 random = 3 } 
				}
			}
			if = {
				limit = {
					liege = {
						job_spiritual = {
							has_children = yes
						}
					}
				}
				liege = {
					job_spiritual = {
						character_event = { id = ko.866 days = 1 random = 3 }
					}
				}
			}
			character_event = { id = ko.872 days = 2 }			
		}
	}
	option = {
		name = EVTOPTC_KO_571.3
		hidden_tooltip = {			
			change_variable = { which = "circle_quality" value = 2 }
			scaled_wealth = -0.1
			character_event = { id = ko.872  days = 3 }
		} 
	}				
}

