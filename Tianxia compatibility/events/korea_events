# Grown-man-napped
character_event = {
	id = ko.991
	desc = EVTDESC_KO_582.15
	picture = GFX_evt_Bloody_man

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582.15
		random_list = {
			70 = {
				character_event = { id = ko.992 } 
			}
			1 = {
				modifier = {
					factor = 25
					is_title_active = e_japan # The title is always ACTIVE, but that does not mean it has a HOLDER; fix this!
				}
				character_event = { id = ko.626 }
				e_japan = {
					holder_scope = { # There is nothing preventing this from being ROOT, which seems like it could cause problems...
						imprison = FROM # This imprisons the holder of e_japan rather than FROM (who also is ROOT); as the holder of e_japan could be a player, randomly being imprisoned for stuff you've not done would be extremely unfun...
					}
				}
			}
			5 = {
				death = {
					death_reason = death_battle
				}
			}			
		}
	}
}

