# Water Purification Raining
narrative_event = {
	id = ko.316
	desc = EVTDESC_KO_253.1
	picture = GFX_evt_water_purification

	is_triggered_only = yes
	
	immediate = {
		any_child  = { # There is no guarantee that you have at least one child with this title...
			limit = {
				is_alive = yes
				has_minor_title = title_najeong
			}
			save_event_target_as = najeong		
		}
	}
	
	option = {
		name = EVTOPTA_KO_253.1
		random_list = {
			50 = { 
				piety = 50
				event_target:najeong  = {
					piety = 20
				}
			}
			50 = { 
				capital_scope = {
					add_province_modifier = {
						name = religious_unrest
						days = 365
					} 
				}
			}
		}							
	}
	
	option = {
		name = EVTOPTB_KO_253.1
		random_list = {
			10 = {
				add_trait = craven
			} 	
			90 = {}
		}						
	}		
}

# Water Purification Blood
narrative_event = {
	id = ko.317
	desc = EVTDESC_KO_250.5
	picture = GFX_evt_water_purification

	is_triggered_only = yes

	immediate = {
		any_child  = { # There is no guarantee that you have at least one child with this title...
			limit = {
				is_alive = yes
				has_minor_title = title_najeong
			}
			save_event_target_as = najeong		
		}
		random_rival = { # There is no guarantee that you have at least one rival...
			save_event_target_as = najeong_blooder
		}
	}
	
	option = {
		name = EVTOPTA_KO_250.5
		prestige = -100
		random_list = {
			50 = { 
				piety = -50
				event_target:najeong = {
					piety = -20
				}
			}
			50 = { 
				capital_scope = {
					add_province_modifier = {
						name = religious_unrest
						days = 365
					}
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_253.1
		prestige = -100
		piety = -10
		trigger = { intrigue = 10 } 	
		random_list = {
			50 = {
				narrative_event = { id = ko.802 }
			}	
			50 = {
				event_target:najeong_blooder  = {
					character_event = { id = ko.803 }
				}
			}							
		}		
	}
}	

