#Satirical Mask Dance *ripped off Satrical Show
character_event = {
	id = ko.537
	desc = EVTDESC_KO_541
	picture = GFX_evt_jugglers

	only_playable = yes
	capable_only = yes
	prisoner = no
	war = no
	has_character_flag = dano_begun

	trigger = {
		in_command = no
		any_vassal = {
			war = no
			prisoner = no
			age = 20
			reverse_opinion = { who = ROOT value = 20 }
		}
	}
	
	immediate = {
		set_character_flag = dano_event_happened
	}	

	mean_time_to_happen = {
		days = 198
		
		modifier = {
			factor = 0.33
			NOT = { has_character_flag = dano_event_happened }
		}	
		
		modifier = {
			factor = 1.5
			has_character_flag = dano_event_happened
		}			
	}

	option = {
		name = "EVTOPTA_KO_541"
		trigger = {
			NOT = { trait = kind }
			NOT = { trait = gregarious }
		}
		random_vassal = {
			limit = {
				war = no
				prisoner = no
				is_adult = yes
				reverse_opinion = {
					who = ROOT
					value = 20
				}
			}
			opinion = {
				modifier = opinion_disgruntled
				who = ROOT
				years = 5
			}
		}
	}

	option = {
		name = EVTOPTB_KO_541
		tooltip_info = kind
		trigger = {
			trait = kind
			NOT = { trait = gregarious }
		}
		random_vassal = {
			limit = {
				war = no
				prisoner = no
				is_adult = yes
				reverse_opinion = {
					who = ROOT
					value = 20
				}
			}
			opinion = {
				modifier = opinion_relieved
				who = ROOT
				years = 5
			}
		}
	}

	option = {
		name = EVTOPTC_KO_541
		tooltip_info = gregarious
		trigger = {
			trait = gregarious
		}
		random_vassal = {
			limit = {
				reverse_opinion = {
					who = ROOT
					value = 20
				}
			}
			opinion = {
				modifier = gregarious_pos
				who = ROOT
				years = 5
			}
		}
	}
}

# Iranian-Korean fusion dance
character_event = {
	id = ko.538
	desc = EVTDESC_KO_542
	picture = GFX_evt_dancing

	trigger = {
		in_command = no
	has_character_flag = dano_begun
	}
	
	immediate = {
		set_character_flag = dano_event_happened
	}	

	mean_time_to_happen = {
		days = 199
		
		modifier = {
			factor = 0.33
			NOT = { has_character_flag = dano_event_happened }
		}	
		
		modifier = {
			factor = 1.5
			has_character_flag = dano_event_happened
		}			
	}

	option = {
		name = EVTOPTA_KO_542
		random_list = {
			50 = {
				character_event = { id = ko.5556 }
			} 
			50 = {
				character_event = { id = ko.540 }
			}
		}
		random_courtier = {
			limit = {
				is_adult = yes
				is_opposite_sex = ROOT
				NOT = { trait = incapable }
				# This might need checks for inaccessibility, possibly homosexuality, etc.
			}
			save_event_target_as = korean_dance_partner
		}
	}
	option = {
		name = EVTOPTB_KO_542
		tooltip_info = diplomacy
		trigger = { diplomacy = 10 is_female = no }
		character_event = { id = ko.550 }
	}
	option = {
		name = EVTOPTC_KO_542
		tooltip_info = diplomacy
		trigger = { diplomacy = 10 is_female = yes }
		character_event = { id = ko.548 }
	}
	
	option = { 
		name = EVTOPTD_KO_542
		random_list = {
			50 = { 
				add_trait = chaste
			} 
			50 = {}
		}
		spouse = {
			opinion = {
				modifier = opinion_impressed
				who = ROOT
				years = 1
			}
		}				
	}
}

