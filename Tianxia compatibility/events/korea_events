	id = ko.721
	desc = EVTDESC_KO_348
	picture = GFX_evt_chinese_characters

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_348
		set_character_flag = strat_gosu
	}

	option = {
		name = EVTOPTB_KO_348
		set_character_flag = strat_gosu
		job_marshal = {
			prestige = -10
			opinion = {
				modifier = opinion_disappointed
				who = ROOT
				years = 3
			}
			random_list = {
				70 = {}
				10 = {
					add_trait = depressed
				}
				20 = {
					add_trait = wroth
					hidden_tooltip = {
						character_event = { id = 38254 }
					}
				}
			}
		}
	}
