character_event = {
	id = ko.541
	desc = EVTDESC_KO_549
	picture = GFX_evt_whispers

	is_triggered_only = yes
	
	# The options all match; fix this!

	option = {
		name = EVTOPTA_KO_549
		character_event = { id = ko.545 }			
	}
	option = {
