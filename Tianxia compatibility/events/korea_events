	id = ko.1004
	desc = EVTDESC_KO_583.8
	picture = GFX_evt_library_korean

	is_triggered_only = yes
	trigger = {
		has_character_modifier = port_of_calling 
	}
	
	option = {
		name = EVTOPTA_KO_583.8
		FROM = {
			character_event = { id = 1005 }
		}
		hidden_modifier = {
			remove_character_modifier = port_of_calling
			character_event = { id = ko.1022 }
			event_target:fishingvillage = {
				remove_province_modifier = trade_port
				}
		} 		
	}
	option = {
		name = EVTOPTB_KO_583.8
		FROM = {
			character_event = { id = 1006 }
		}	
	}
	option = {
		name = EVTOPTC_KO_583.8
		tooltip_info = diplomacy
		trigger = { diplomacy = 12 }
		FROM = {
			character_event = { id = 1007 }
		}	
	}
	option = {
		name = EVTOPTD_KO_583.8
		trigger = {
			attribute_diff = {
				character = FROM
				attribute = stewardship
				value = 3
			}
		} 
		FROM = {
			character_event = { id = 1008 }
		}	
	}			
}

