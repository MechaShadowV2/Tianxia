		name = EVTOPTA_KO_272 # Why would you pick this instead of C?
		job_marshal = {
			opinion = {
				modifier = opinion_shocked
				who = ROOT
				years = 1
			}
			random_list = {
				25 = {
					add_trait = strong # This is a congenial trait; why is it being added instead of Robust?
				}
				25 = {
					add_trait = stressed
				}
				50 = {}			
			}			
		}		
	}
	option = {
		name = EVTOPTB_KO_272 # Why would this ever be a better option than C?
		piety = 5
	}
	option = {
		name = EVTOPTC_KO_272
		military_techpoints = 25
	}		
}

