# Romancing Dance Partner MALE
character_event = {
	id = ko.065
	desc = EVTDESC_KO_018
	picture = GFX_evt_romance_at_market

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_018
        character_event = { id = ko.059}
	}	
	option = {
		name = EVTOPTB_KO_018
		hidden_tooltip = {
			if = {
				limit = {
					OR = {
						trait = gregarious
						trait = hedonist
						trait = quick
					}
				}
				random_list = {
					80 = { character_event = { id = ko.066 } } 
					20 = { character_event = { id = ko.067 } } 
				}
			}
			if = {
				limit = {
					OR = {
						trait = shy
						trait = ugly
					}
				}
				random_list = {
					30 = { character_event = { id = ko.066 } } 
					70 = { character_event = { id = ko.067 } } 
				}
			}
			if = {
				limit = {
					NOT = { trait = gregarious }
					NOT = { trait = hedonist }
					NOT = { trait = quick }
					NOT = { trait = shy }
					NOT = { trait = ugly }
				}
				random_list = {
					50 = { character_event = { id = ko.066 } } 
					50 = { character_event = { id = ko.067 } } 
				}
			}
		}
	}
}

# Flirting success
character_event = {
	id = ko.066
	desc = EVTDESC_KO_019
	picture = GFX_evt_whisper_black_hair

	is_triggered_only = yes
	option = {
		name = EVTOPTA_KO_019 # This does nothing; is that intentional?
	}
	option = {
		name = EVTOPTB_KO_019
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = yes
			age = 17
			trait = indulgent_wastrel
			trait = fair
			trait = quick
			trait = lustful
			attributes = {
				intrigue = 10
			}
		}
	}
	# There appears to be a missing check for whether you're homosexual in this event chain since this option, as written, will give you a homosexual female lover if you're a male homosexual...
	# There probably also should be checks for this MUCH earlier to prevent other weird gender combinations
	option = {
		name = EVTOPTC_KO_019
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = yes
			age = 17
			trait = intricate_webweaver
			trait = fair
			trait = quick
			trait = lustful
			attributes = {
				intrigue = 10
			}
		}
		new_character = {
			if = {
				limit = { ROOT = { trait = homosexual } }
				add_trait = homosexual
			}
			add_lover = ROOT
		}
	}
}

