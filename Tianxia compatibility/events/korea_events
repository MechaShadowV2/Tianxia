character_event = {
	id = ko.513
	desc = EVTDESC_KO_513
	picture = GFX_evt_mountain_shrine

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_513 # This does nothing; is that intentional?
	}
	option = {
		name = EVTOPTB_KO_513 # Identical to the above; why?
	}
	option = {
		tooltip_info = stutter
		trigger = { trait = stutter }
		name = EVTOPTC_KO_513
		random_list = {
			50 = {
				remove_trait = stutter # Congenial traits are *very* rarely removed in vanilla; is it justifiable to do it here?
			}
			50 = {}
		}
	}
}

