# The bell is completed
narrative_event = {
	id = ko.591
	title = "EVTTITLE_KO_591"
	desc = "EVTDESC_KO_591"
	major = yes
	culture = korean
	
	hide_from = yes
	
	picture = "GFX_evt_graveyard"
	
	only_playable = yes
	
	trigger = {
		year = 770
		has_landed_title = c_gyeongju
		NOT = {	year = 780 }
		NOT = {	year = 1066 }
		NOT = {	has_global_flag = bul_bell_done } # Is this properly set if you start later?
	}
	
	major_trigger = {
		ai = no
		culture_group = koreanic
		NOT = { religion_group = christian }
		NOT = { religion_group = muslim }
		NOT = { religion = tengri_pagan }
		NOT = { religion = tengri_pagan_reformed }
	}	
	is_triggered_only = yes	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 1.5
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
		modifier = {
			factor = 0.3
			trait = architect
		}
		}
	
	option = {
		name = "EVTOPTA89500"
		set_global_flag = bul_bell_done
	}
}

# Ask marshal for help
character_event = {
	id = ko.592
	
	picture = GFX_evt_war_planning
	border = GFX_event_normal_frame_war
	
	desc = EVTDESC_KO_337
	only_playable = yes
	capable_only = yes
	prisoner = no
	is_triggered_only = yes
	
	# This event can trigger throughout the game; is this intentional?
	
	trigger = {
		has_focus = focus_war
		can_have_more_leadership_traits = yes
		NOT = { has_character_flag = strat_gosu }
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 1.25
			trait = misguided_warrior
		}
		modifier = {
			factor = 1.5
			trait = tough_soldier
		}
		modifier = {
			factor = 2.0
			trait = skilled_tactician
		}
		modifier = {
			factor = 3.0
			trait = brilliant_strategist
		}
		modifier = {
			factor = 0.1
			NOT = { martial = 5 }
		}
		modifier = {
			factor = 0.5
			NOT = { martial = 8 }
		}
		modifier = {
			factor = 2.0
			NOT = { martial = 12 }
		}
		modifier = {
			factor = 2.0
			has_character_modifier = adept_of_strategy
		}
		modifier = {
			factor = 3.0
			trait = strategist
		}
		modifier = {
			factor = 0.25
			leader_traits = 1
		}
	}
	
	option = {
		name = EVTOPTA_KO_337 # Ready, Sem
		set_character_flag = martial_hagseng
	}
}

# Discussing Baekje-Gogoreyo
character_event = {
	id = ko.593
	picture = GFX_evt_war_planning
	border = GFX_event_normal_frame_war
	desc = EVTDESC_KO_338
	only_playable = yes
	capable_only = yes
	prisoner = no
	is_triggered_only = yes
	
	# Is this something that actually makes sense throughout the game, given that Baekye ang Gogoreyo might not be around all the time...?
	
	trigger = {
		has_focus = focus_war
		has_character_flag = martial_hagseng
		can_have_more_leadership_traits = yes
		OR = {
			NOT = { trait = experimenter }
			NOT = { has_character_flag = strat_gosu }
			AND = {
				NOT = { trait = holy_warrior }
				trait = zealous
			}
		}
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 1.25
			trait = misguided_warrior
		}
		modifier = {
			factor = 1.5
			trait = tough_soldier
		}
		modifier = {
			factor = 2.0
			trait = skilled_tactician
		}
		modifier = {
			factor = 3.0
			trait = brilliant_strategist
		}
		modifier = {
			factor = 0.1
			NOT = { martial = 5 }
		}
		modifier = {
			factor = 0.5
			NOT = { martial = 8 }
		}
		modifier = {
			factor = 2.0
			NOT = { martial = 12 }
		}
		modifier = {
			factor = 2.0
			has_character_modifier = adept_of_strategy
		}
		modifier = {
			factor = 3.0
			trait = strategist
		}
		modifier = {
			factor = 0.25
			leader_traits = 1
		}
	}
	
	option = {
		name = EVTOPTA_KO_338 # Buddhism
		character_event = { id = ko.594 }
	}
	option = {
		name = EVTOPTB_KO_338 # Navy
		character_event = { id = ko.595 } # Should this require a port...?
	}
	option = {
		name = EVTOPTC_KO_338 # Farming
		character_event = { id = ko.596 }
	}
}

