# Villager and the Tiger 2
narrative_event = {
	id = ko.402
	title = EVTTITLE_KO_171
	desc = EVTDESC_KO_171
	picture = GFX_evt_story_tiger

	is_triggered_only = yes
	
	option = {
		tooltip_info = deceitful
		trigger = { trait = deceitful }
		name = EVTOPTA_KO_171
		hidden_tooltip = {
			narrative_event = { id = ko.403	}
			random_child = {
				random_list = {
					20 = {
						add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
					}
					20 = {
						add_trait = playful # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
					}
					5 = {
						add_trait = ambitious
					}
					20 = {
						remove_trait = craven
					}
					20 = {
						opinion = {
							modifier = opinion_idiot
							who = ROOT
							years = 3
						}
					}
					15 = {}
				}
			} 	 
		}	
	}
	option = {
		name = EVTOPTB_KO_171
		hidden_tooltip = {
			narrative_event = { id = ko.403 }
		}		
	}		
	option = {
		name = EVTOPTC_KO_171
		tooltip_info = diligent
		trigger = { trait = diligent }
		hidden_tooltip = {
			narrative_event = { id = ko.403 } 
			random_list = {
				10 = {
					change_stewardship = 1
				}
				20 = {
					change_stewardship = 2
				}
				70 = {}
			}
		}
	}
	option = {
		name = EVTOPTD_KO_171
		trigger = { religion = buddhist }
		hidden_tooltip = {
			narrative_event = { id = ko.403 }
		} 
		piety = 10
		prestige = 20
	}
}

