	picture = GFX_evt_k_villagers

	is_triggered_only = yes
	option = {
		name = EVTOPTA_KO_901
		hidden_tooltip = {
			random_list = {
				20 = {
					modifier = {
						factor = 3
						trait = one_legged
					}
					character_event = { id = ko.1041 }
				}
				20 = {
					modifier = {
						factor = 3
						trait = wroth
					}
					character_event = { id = ko.1048 }
				}
				20 = {
					modifier = {
						factor = 3
						trait = robust
					}
					character_event = { id = ko.1046 }
				}
				20 = {
					modifier = {
						factor = 3
						trait = patient
					}
					character_event = { id = ko.1041 }
				}
				20 = {
					character_event = { id = ko.1041 } #plain win
				}
				20 = {
					character_event = { id = ko.1045 } #grandma veteran tricks
				}	
				20 = {
					character_event = { id = ko.1047 } #plain lose
				}
			}
		}
	}	
}

# Chicken Game Win Patience
character_event = {
	id = ko.1041
	
	# The descs aren't exclusive; fix this!
	
	desc = {
		text = EVTDESC_KO_902
		trigger = { ROOT = { trait = patient } }
	}
	desc = {
		text = EVTDESC_KO_902.2
		trigger = { ROOT = { trait = one_legged } }
	}
		desc = {
		text = EVTDESC_KO_902
		trigger = { ROOT = { is_alive = yes } } # ROOT will always be alive if ROOT is getting events...
	}
	
	# These descs lack triggers; fix that!
	desc = EVTDESC_KO_902
	desc =  EVTDESC_KO_902
	picture = GFX_evt_k_villagers

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_902
		character_event = { id = ko.1042 }
	}
	option = {
		name = EVTOPTB_KO_902
		character_event = { id = ko.1043 }
