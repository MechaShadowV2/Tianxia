# Lord Al'chon
narrative_event = {
	id = ko.404
	desc = EVTDESC_KO_173
	title = EVTTITLE_KO_173
	picture = GFX_evt_meeting_on_mountain

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_173
		hidden_tooltip = {
			narrative_event = { id = ko.405 }
		} 	
		random_child = {
			random_list = {
				50 = { 
					add_character_modifier = {
						name = the_naturalist
						duration = 365
					} 
				}
				30 = {}
				20 = {
					add_trait = haughty # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
				}		
			}
		}
	}
	option = {
		name = EVTOPTB_KO_173
		hidden_tooltip = {
			narrative_event = { id = ko.405 }
		} 
	}
	option = { 
		name = EVTOPTC_KO_173
		trigger = { NOT = { dynasty = 1000078000 } } # This implies that some other option should be dynasty-specific, which isn't the case...
		hidden_tooltip = {
			narrative_event = { id = ko.405 }
		} 
	}
	option = { 
		name = EVTOPTD_KO_173
		tooltip_info = learning
		trigger = { learning = 15 }
		any_courtier = { # Not random?
			limit = {
				is_female = yes
			}
			random_list = {
				20 = {
					add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
				}
				20 = {
					add_trait = idolizer # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
				}
				20 = {
					add_trait = ambitious
				}
				20 = {
					remove_trait = zealous
				}
				20 = {
					opinion = {
						modifier = opinion_enlightened
						who = ROOT
						years = 3
					}
				}
				40 = {}
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.405 }
		} 
	}
}


