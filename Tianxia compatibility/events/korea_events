character_event = {
	id = ko.972
	desc = EVTDESC_KO_582
	picture = GFX_evt_japanese_pirates

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_582
		random_list = {
			# Any character with martial X+Y (Y>0) will fulfill the conditions for modifiers for martial X too, making characters with high skill have increased chance of the low skill options too
			10 = {
				modifier = {
					factor = 1 # This is pointless
					martial = 4
				}  
				character_event = { id = ko.973 } #capture pirate
			}
			10 = {
				modifier = {
					factor = 2
					martial = 8
				} 
				character_event = { id = ko.974 } #they flee
			}
			10 = {
				modifier = {
					factor = 3
					martial = 12
				} 
				character_event = { id = ko.979 } #they flee 
			}
			10 = {
				modifier = {
					factor = 3
					martial = 10
				}
				character_event = { id = ko.983 } #they flee 2
			}
			10 = {
				modifier = {
					factor = 4
					martial = 16
				}
				character_event = { id = ko.982 } #request for Shaman.
			}
		}
	}
	option = {
		name = EVTOPTB_KO_582
		prestige = 50
		character_event = { id = ko.986 }
	}
	option = {
		name = EVTOPTC_KO_582
		prestige = -100
		random_list = {
			50 = {
