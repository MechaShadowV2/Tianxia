# Undercutting Mu
character_event = {
	id = ko.847
	desc = EVTDESC_KO_569
	picture = GFX_evt_holy_paper

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_569
		random_list = {
			50 = {
				modifier = {
					factor = 3
					trait = arbitrary
				} 
				modifier = {
					factor = 3
					trait = greedy
				}
				modifier = {
					factor = 3
					trait = shrewd # But not Quick/Genius?
				}
				modifier = {
					factor = 2
					piety = 60
				}
				modifier = {
					factor = 3
					piety = 0
				}
				modifier = {
					factor = 4
					piety = -50
				}
				modifier = {
					factor = 4
					learning = 2 # The majority of the game's playable adult characters have way more than 2 in any given stat
				}
				modifier = {
					factor = 5
					piety = -100
				}
				character_event = { id = ko.848 }
			}
			50 = {
				modifier = {
					factor = 3
					trait = temperate
				} 
				modifier = {
					factor = 3
					trait = zealous
				}
				modifier = {
					factor = 3
					trait = charitable
				}
				modifier = {
					factor = 2 
					piety = 100 # A character with 100 piety or more will still get the increased chance from <100 piety for the other option...
				}
				modifier = {
					factor = 3
					piety = 200
				}
				modifier = {
					factor = 4
					piety = 300
				}
				modifier = {
					factor = 4
					learning = 8
				}
				modifier = {
					factor = 5
					piety = 400
				}
				character_event = { id = ko.849 }
			}
		} 
	}
	option = {
		name = EVTOPTB_KO_569
		piety = 10
		add_character_modifier = {
			name = despised_aristocrat
			duration = 365
		}
	}
	option = {
		name = EVTOPTC_KO_569
		add_character_modifier = {
			name = town_appreciate
			duration = 365
		}			
  		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = yes
			age = 52
			trait = deceitful
			trait = greedy
			trait = quick
			trait = midas_touched
			attributes = {
			intrigue = 10
			stewardship = 10
			}
		}
		hidden_tooltip = {
			new_character = {
				save_event_target_as = dodgy_mu
			}
		}		
	}
	option = {
		name = EVTOPTD_KO_569
		tooltip_info = greedy
		trigger = { trait = greedy }
		scaled_wealth = 10 # This is 10 years of income; is this reasonable?
		hidden_tooltip = {
			random_list = {
				30 = {
					character_event = { id = ko.850 days = 50 random = 20 }
				}
				70 = {}
			}
		}
	}				
}

