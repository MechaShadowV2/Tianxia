# Piracy
character_event = {
	id = ko.1010
	desc = EVTDESC_KO_583.9
	picture = GFX_evt_japanese_pirates

	is_triggered_only = yes
	trigger = {
		has_character_modifier = port_of_calling 
	}
	
	option = {
		name = EVTOPTA_KO_583.9
		random_realm_province = {
			limit = {
				port = yes # Are we sure that this province is reachable by sea from (presumably) the fishing village province?
			} 
			add_province_modifier = {
				name = marauding_pirates
				duration = 365
			}
		}
		random_realm_province = {
			limit = {
				port = yes # Are we sure that this province is reachable by sea from (presumably) the fishing village province?
			} 
			add_province_modifier = {
				name = marauding_pirates
				duration = 256
			}
		}
		random_realm_province = {
			limit = {
				port = yes # Are we sure that this province is reachable by sea from (presumably) the fishing village province?
			} 
			add_province_modifier = {
				name = marauding_pirates
				duration = 484
			}
		}
		random_realm_province = {
			limit = {
				port = yes # Are we sure that this province is reachable by sea from (presumably) the fishing village province?
			} 
			add_province_modifier = {
				name = marauding_pirates
				duration = 625
			}
		}			
	}
	option = {
		name = EVTOPTB_KO_583.9
		hidden_modifier = {
			remove_character_modifier = port_of_calling
			character_event = { id = ko.1022 }
			event_target:fishingvillage = {
				remove_province_modifier = trade_port
			}
		} 	
	}
}

