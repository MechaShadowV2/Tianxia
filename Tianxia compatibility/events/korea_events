# They flee
character_event = {
	id = ko.979
	desc = EVTDESC_KO_582.4
	picture = GFX_evt_battle

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_582.4
		prestige = 100
		random_demesne_province = {
			limit = {
				is_capital = yes # This makes random_province useless!
			}
			add_province_modifier = {
				name = missing_reinforcements
				duration = 365
			}
		}
		random_list = {
			50 = {
				change_martial = 1
			}
			50 = {}
		}
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}			
	}
	
	option = {
		name = EVTOPTB_KO_582.4
		wealth = -120 # This is quite expensive for what you get...
		random_demesne_province = {
			limit = {
				is_capital = yes # This makes random_province useless!
			}
			add_province_modifier = {
				name = missing_reinforcements
				duration = 365
			}
		}
		event_target:fishingvillage = {
			set_province_flag = recent_depopulation_1 # This does nothing; of course, it shouldn't do anything if RD isn't active, but...
		}	
		event_target:fishingvillage = {
			owner = {
				spawn_unit = {
					owner = ROOT
					province = ROOT
					home = ROOT
					troops = {
						archers = { 100 500 }
						light_infantry = { 100 200 }
					}
					attrition = 0.3
					maintenance_multiplier = 0.5
					reinforces = yes
					reinforce_rate_multiplier = 0.33
				}
			}
		}
	}
	option = {
		name = EVTOPTC_KO_582.4
		random_demesne_province = {
			limit = {
				is_capital = yes # This makes random_province useless!
			}
			add_province_modifier = {
				name = missing_reinforcements
				duration = 365
			}
		}			
		wealth = -50
	}			
}

