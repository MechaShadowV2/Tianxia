	id = ko.091
	desc = EVTDESC_KO_062
	picture = GFX_evt_scribe
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_062
		set_global_flag = k_alphabet_reformed
		prestige = 500
		piety = 300
		scaled_wealth = -0.2
		any_vassal = {
			limit = { 
				NOT = {
					religion = buddhist # Shouldn't Hindu, Jain, Dravidian, and Indo-Aryan vassals also be fine with it?
				}
			}
			opinion = {
				modifier = opinion_unreasonable
				who = ROOT
				years = 10
			}			
		}
	}
	option = {
		name = EVTOPTB_KO_062	
	}
}

# Invent Hangul
character_event = {
	id = ko.092
	desc = EVTDESC_KO_063
	picture = GFX_evt_scribe
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_063
		trigger = {
			NOT = {
				trait = genius # Without this, you can pick the worse option if you live up to the conditions for the better option...
			}
		}
		hidden_tooltip = {
			character_event = { id = ko.093 }
		}		
		prestige = 2000
		change_learning = 5 # This is HUGE; is it at all reasonable?
		scaled_wealth = -0.2
		set_global_flag = k_alphabet_reformed
		any_vassal = {
			limit = {
				is_republic = yes
			}
			opinion = {
				modifier = opinion_gift_from_above
				who = ROOT
				years = 50
			}
		}
	}
			
	option = {
		tooltip_info = genius
		trigger = { trait = genius }
		name = EVTOPTB_KO_063
		hidden_tooltip = {
			character_event = { id = ko.093 }
		}
		prestige = 2500 # 500 extra prestige is a rather small boost for a relatively rare trait...
		change_learning = 5 # This is HUGE; is it at all reasonable?
		scaled_wealth = -0.2
		any_vassal = {
			limit = {
				is_republic = yes
			}
			opinion = {
				modifier = opinion_gift_from_above
				who = ROOT
				years = 50
			}
		}
	}
}

