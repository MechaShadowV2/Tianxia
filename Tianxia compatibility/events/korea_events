			has_landed_title = d_gyeonggi
			has_landed_title = c_hanseong
			has_landed_title = c_namyangju
			has_landed_title = c_chunju
			has_landed_title = c_hwaseong
		}
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = gregarious
		}
		modifier = {
			factor = 0.5
