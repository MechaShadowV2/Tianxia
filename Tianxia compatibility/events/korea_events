		name = EVTOPTA_KO_118.b
		random_list = { 
			20 = {
				imprison = ROOT # Why is ROOT imprisoning ROOT?
			}
			80 = {}
		}	
	}
	
	option = {
		name = EVTOPTB_KO_118.b # This has no effect; why?
		tooltip_info = diplomacy
		trigger = { diplomacy = 10 }
	}
	
	option = {
		name = EVTOPTC_KO_118.b
		random_list = { 
			50 = {
				imprison = ROOT # Why is ROOT imprisoning ROOT?
			}
			50 = {}
		}	 
		add_trait = ambitious
	}
	
	option = {
		name = EVTOPTD_KO_118.b # Why would you risk picking this option when the others are less dangerous?
		d_gaya = {
			destroy_landed_title = THIS
		}
		random_list = { 
			10 = {
				death = {
					death_reason = death_execution_beheading
					# No killer is set!
				}
			}
			90 = {}
		}	
	}	
}

# Remake the confederacy
narrative_event = {
	id = ko.274
	desc = EVTDESC_KO_118.c
	picture = GFX_evt_courtiers_talking
	only_playable = yes
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_118.c
		prestige = 200
		piety = -200 # Why do you get a duchy for piety?
		activate_title = { title = d_gaya status = yes } # There are no checks for d_gaya having another holder...
		d_gaya = {
			grant_title = ROOT
		}	
	}
	
	option = {
		name = EVTOPTB_KO_118.c
		prestige = 200
		wealth = 200
		piety = -200 # Why do you get a duchy for piety?
		activate_title = { title = d_gaya status = yes } # There are no checks for d_gaya having another holder...
		d_gaya = {
			grant_title = ROOT
		}
		create_family_palace = yes
		set_government_type = merchant_republic_government # Where's the check for having The Republic (and a port)? This is an instant game over otherwise!
		# Does it make sense to have an European-style MR (and European-style MR clothes) in Korea?
		if = {
			limit = {
				NOT = {
					num_of_government_vassals = {
						government = merchant_republic_government
						value = 1
					}
				}
			}
			any_unique_dynasty_vassal = { # gives random vassals a family palace
				count = 4
				limit = {
					is_female = no
					OR = {
						is_republic = yes
						is_tribal = yes
					}
					is_patrician = no
					prisoner = no
					NOT = { trait = incapable }
					is_adult = yes
					NOT = { 
						dynasty = none 
					}
				}
			}
		}
	}
	option = {
		name = EVTOPTC_KO_118.c
		top_liege = {
			opinion = {
				modifier = opinion_loyal_servant
				who = ROOT
				years = 5
			}
		}
		hidden_tooltip = {
			random_list = { 
				33 = {
					character_event = { id = ko.302  }
				}
				34 = {
					character_event = { id = ko.303  }
				}
				33 = {
					character_event = { id = ko.304  }
				}
			}
		}
		add_trait = deceitful
		piety =-50
	}
}

# Another tattoo
character_event = {
	id = ko.275
	desc = EVTDESC_KO_115.v
	picture = GFX_evt_market_sinic
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		has_character_flag = has_tattoo
		NOT = { has_character_flag = has_tattootwo }
		# Is this strict enough?
		OR = {
			has_landed_title = d_gyeonggi
			has_landed_title = c_hanseong
			has_landed_title = c_namyangju
			has_landed_title = c_chunju
			has_landed_title = c_hwaseong
		}
	}
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = diligent    
		}
		modifier = {
			factor = 0.5
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 2
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = shy
		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
	}
	option = {
		name = EVTOPTA_KO_115.v
		wealth = -10
		set_character_flag = has_tattootwo
		spouse = {
			opinion = {
				modifier = opinion_lover # Lovers tend to have no time limit on that relationship...
				who = ROOT
				years = 20
			}
		}
		any_consort = {
			opinion = {
				modifier = opinion_love_rival
				who = ROOT
				years = 1
			}
		}	
	}
	option = {
		name = EVTOPTB_KO_115.v
		wealth = -10
		set_character_flag = has_tattootwo
		add_trait = sympathy_indian
	}
	option = {
		name = EVTOPTC_KO_115.v
		wealth = -10
		set_character_flag = has_tattootwo
		any_courtier = { # Not random?
			limit = {
				dynasty = 1000155023 # Why?
			}
			change_intrigue = 3 # This is a pretty large gain of base intrigue...
			opinion = {
				modifier = opinion_mutual_respect
				who = ROOT
				years = 3
			}
		}		
	}
	option = {
		name = EVTOPTD_KO_115.v
		wealth = -10
		set_character_flag = has_tattootwo
		random_child = {
			limit = {
				is_female = no
			} 
			opinion = {
				modifier = opinion_mutual_respect
				who = ROOT
				years = 20
			}
		}
	}			
}

