			} 
			50 = {
				character_event = { id = ko.540	}
			}
		}
		random_courtier = {
			limit = {
				is_adult = yes
				is_opposite_sex = ROOT
				NOT = { trait = incapable }
				# This might need checks for inaccessibility, possibly homosexuality, etc.
			}
			save_event_target_as = korean_dance_partner
		}
	}
	option = {
		character_event = { id = ko.545 }
		name = EVTOPTB_KO_544		
	}
}

