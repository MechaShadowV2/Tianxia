# Gaya Duchy creation
narrative_event = {
	id = ko.272
	desc = EVTDESC_KO_118.a
	picture = GFX_evt_agriculture
	only_playable = yes
	culture_group = koreanic
	is_triggered_only = yes
	
	trigger = {
		# Is this strict enough?
		in_command = no # Why is this not in the triggers for similar events?
		NOT = { has_landed_title = d_gaya }
		NOT = { has_landed_title = k_korea } # This title no longer exists; fix!
		NOT = { has_landed_title = k_balhae }
		OR = {
			has_landed_title = c_jinju
			has_landed_title = c_busan
		}
		has_character_flag = dano_begun
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 0.5
			trait = diligent    
		}
		modifier = {
			factor = 2
			trait = slothful
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 2
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 2
			trait = shy
		}
		modifier = {
			factor = 1.5
			has_character_flag = dano_event_happen
		}	
		modifier = {
			factor = 0.33
			NOT = { has_character_flag = dano_event_happen }
		}	
	}
	
	option = {
		name = EVTOPTA_KO_118.a # This has no effect; why?
		ai_chance = {
			factor = 50
		}	
	}
	
	option = {
		name = EVTOPTB_KO_118.a # There is no check for d_gaya not having another holder; why?
		piety = -200 # Why do you get a duchy title for piety?
		character_event = { id = ko.273 days = 120 }
		activate_title = { title = d_gaya status = yes }
		d_gaya = {
			grant_title = ROOT
		}
		ai_chance = {
			factor = 50
			modifier = {
				factor = 0
				NOT = { trait = ambitious  }
			}
		}
	}
	option = {
		name = EVTOPTC_KO_118.a
		trigger = { NOT = { year = 780 } } # Why?
		ai_chance = {
			factor = 50
			modifier = {
				factor = 0
				NOT = { trait = ambitious }
			}
		}
		narrative_event = { id = ko.274 }
	}
}

