	title = EVTTITLE_KO_165
	picture = GFX_evt_hwajeo

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_165
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		tooltip_info = zealous
		trigger = { trait = zealous }
		name = EVTOPTB_KO_165
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
		random_child = {
			random_list = {
				20 = {
					add_trait = cynical
				}
				10 = {
					change_learning = 1
				}
				20 = {
					add_trait = zealous
				}
				10 = {
					opinion = {
						modifier = opinion_idiot
						who = ROOT
						years = 3
					}
				}
				40 = {}
			}
		} 	
	}
	option = {
		tooltip_info = gluttonous
		trigger = { trait = gluttonous }
		name = EVTOPTC_KO_165
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
		random_child = {
			opinion = {
				modifier = gluttonous_pos
				who = ROOT
				years = 3
			}
		}  
	}
}

