				is_republic = yes
			}
			opinion = {
				modifier = opinion_agreed_with_opinion
				who = ROOT
				years = 1
			}
		}
		random_demesne_province = {
			limit = {
				is_capital = yes # Why look for a random province and then limit it to the capital?
			}
			add_province_modifier = {
				name = peasants_scorned
				duration = 360
			}
		}
	}
	option = {
		name = EVTOPTB_KO_642
		any_vassal = { # Not random?
			limit = {
				is_republic = yes
			}
			opinion = {
				modifier = opinion_disagreed_with_opinion
				who = ROOT
				years = 1
			}
		}
		random_demesne_province = {
			limit = {
				is_capital = yes # Why look for a random province and then limit it to the capital?
			}
			add_province_modifier = {
				name = inspired_peasantry
				duration = 360
			}
		}
	}
	option = {
		name = EVTOPTC_KO_642
		random_list = {
			50 = {
				add_trait = arbitrary
			} 
			50 = {
				add_trait = just
			} 
		}
	}
}

