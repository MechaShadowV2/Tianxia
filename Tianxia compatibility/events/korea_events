# Wa pirates spot
character_event = {
	id = ko.620
	desc = EVTDESC_KO_620
	picture = GFX_evt_underwater_king
	
	# Is it certain that this makes sense? There might not be a reason to believe there are Wa pirates around if the political landscape is sufficiently different...

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_620
		if = {
			limit = {
				NOT = { martial = 15 }
			}
			hidden_tooltip = {
				character_event = { id = ko.622 }
			}	
		}
		if = {
			limit = {
				NOT = { martial = 10 }
			}
			hidden_tooltip = {
				character_event = { id = ko.621 }
			}
		}
		if = {
			limit = {
				NOT = { martial = 5 }
			}
			hidden_tooltip = {
				character_event = { id = ko.623 }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_620
		random_list = {
			15 = { character_event = { id = ko.627 } }
			15 = { character_event = { id = ko.628 } }
			15 = { character_event = { id = ko.629 } }
			15 = { character_event = { id = ko.630 } }
			20 = { character_event = { id = ko.631 } }
			20 = { character_event = { id = ko.632 } }
		}
	}
	option = {
		name = EVTOPTC_KO_620
		if = {
			limit = {
				is_alive = yes
				prestige = 4000
			}
			hidden_tooltip = {
				character_event = { id = ko.634 }
			}
		}
		if = {
			limit = { is_alive = yes }
			hidden_tooltip = {
				character_event = { id = ko.633 }
			}
		}
	}
}

