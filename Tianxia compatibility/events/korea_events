		}
		modifier = {
			factor = 2
			trait = kind
		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
	}
	
	option = {
		name = EVTOPTA_KO_424
		clr_character_flag = daughter_marshal_kdrama
		job_marshal = { # There is nothing guaranteeing that this is the same marshal
			add_friend = event_target:infatuated_daughter
		}		
		remove_trait = paranoid
 	}			
}

# K Drama ender
character_event = {
	id = ko.755

	is_triggered_only = yes
	
	# There is nothing guaranteeing that this event triggers before one of the other events, so things would break
	
	trigger = {
		OR = {
			event_target:infatuated_daughter = {
				is_alive = no
			}
			event_target:infatuated_marshal = {
				is_alive = no 
			}
		}
	}
	
	# These weight multipliers, and several earlier ones, don't matter because the event is guaranteed to trigger on_yearly_pulse as it isn't a random event!
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 2
			trait = paranoid  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
	}
		
	hide_window = yes

	immediate = {
		clr_character_flag = daughter_marshal_kdrama
	}
}

# Equivilant of Kim-Busik (next event id used)
narrative_event = {
	id = ko.756
	title = "EVTTITLE_KO_591.2"
	desc = "EVTDESC_KO_591.2"
	culture = korean
	
	picture = "GFX_evt_korean_scholar"
	
	# How certain can we be that this event actually makes sense if the game starts well before 1125?
	
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		year = 1125
		ai = no
		culture_group = koreanic
		has_landed_title = c_gyeongju
		NOT = {	year = 1200 }
		NOT = { has_global_flag = sam_guk_done }
		NOT = { religion_group = christian }
		NOT = { religion_group = muslim }
		NOT = { religion = tengri_pagan }
		NOT = { religion = tengri_pagan_reformed }		
	}
	
	weight_multiplier = {
		days = 1
		modifier = {
			factor = 2
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 0.5
			trait = patient
		}
		modifier = {
			factor = 3
			trait = wroth
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
	}
	
	
	immediate = {
		any_vassal  = {
			limit = {
				is_alive = yes
				learning = 13
			}
			save_event_target_as = sakgugsagi
		}	
	} 
	
	option = {
		name = EVTOPTA_KO_591.2
			event_target:sakgugsagi = {
			opinion = {
				modifier = opinion_flattered
				who = ROOT
				years = 5
			}
		}	
		set_global_flag = sam_guk_done
	}
	option = {
	trigger = { NOT = { dynasty = 1000078000 } } # Why is there a dynasty-specific option? Vanilla almost never does dynasty-specific things...
		name = EVTOPTB_KO_591.2
		add_trait = wroth
		any_vassal = {
			limit = { dynasty = 1000078000 }
			opinion = {
				modifier = opinion_unappreciated
				who = ROOT
				years = 5
			}
		}		
		set_global_flag = sam_guk_done
	}
	option = {
		name = EVTOPTC_KO_591.2
		set_global_flag = sam_guk_done
		prestige = 50
		event_target:sakgugsagi = {
			opinion = {
				modifier = opinion_ignorant
				who = ROOT
				years = 5
			}
		}			
	}
	option = {
		name = EVTOPTD_KO_591.2
		change_learning = 1
		piety = -50
		set_global_flag = sam_guk_done
	}	
}
	

# Harvest Moon dance
character_event = {
	id = ko.243
	desc = EVTDESC_KO_099
	picture = GFX_evt_chuseok_dance
	is_triggered_only = yes

	immediate = {
		# There is nothing checking that such a child exists before the event is triggered...
		random_child = {
			limit = {
				is_alive = yes
				is_female = yes
				age = 8
				NOR = {
					age = 20
				}
			}
			save_event_target_as = harvestdancer
		}
	}
	
	option = {
		name = EVTOPTA_KO_099 # This has no effect; why?
	}
	option = {
		trigger = {
			event_target:harvestdancer = { 
				is_alive = yes # This is always true if the target is set, since there is no chance of death between the immediate block and the option being visible
			}
		}	
		name = EVTOPTB_KO_099
		event_target:harvestdancer  = {
			random_list = {
				10 = {}		
				60 = {
					character_event = { id = ko.244 }
				}
				30 = {
					add_character_modifier = {
						modifier = great_dancer
						years = 35
					} 
				}
			}
		}
	}
}

