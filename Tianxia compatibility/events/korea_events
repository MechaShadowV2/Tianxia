# Core strength
character_event = {
	id = ko.1046	
	desc = EVTDESC_KO_902.4
	picture = GFX_evt_relaxed_ruler

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_902.4
		add_character_modifier = {
			name = peasants_protected # Isn't this an RD modifier?
			duration = 2000
		}
	}
	
	option = {
		name = EVTOPTB_KO_902.4 # Why is there no effect for this option since it is trait-restricted?
		tooltip_info = lustful
		trigger = { trait = lustful }
	}
}

