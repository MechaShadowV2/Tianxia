# Water Purification Bandits Son
character_event = {
	id = ko.705
	desc = EVTDESC_KO_257
	picture = GFX_evt_bandits_forest

	is_triggered_only = yes
	
	immediate = {
		any_child  = {
			limit = {
				is_alive = yes
				has_minor_title = title_najeong
			}
			save_event_target_as = najeong		
		}
	}
	
	option = {
		name = EVTOPTA_KO_257
		wealth = 20
		random_list = {
			80 = {}
			20 = {
				add_trait = arbitrary
			}
		}	
	}
	option = {
		name = EVTOPTB_KO_257
		event_target:najeong = {
			opinion = {
				modifier = opinion_delighted
				who = ROOT
				years = 1
			} 
		}
	}
	option = {
		name = EVTOPTC_KO_257
		event_target:najeong = {
			opinion = {
				modifier = opinion_delighted
				who = ROOT
				years = 2
			}	
		}
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = no
			age = 13
			trait = brave
			trait = fair
			trait = quick
			attributes = {
			intrigue = 10
			}
		}
		hidden_tooltip = {
			new_character = {
				add_lover = event_target:najeong # The Najeong title CANNOT be granted to adults, and CK2 NEVER adds lovers for anyone under 16 even if they fall in love; FIX THIS AND EVERYTHING RELATED TO IT!
			}
		}			
	}
}

