# Notify target of friendly snake
character_event = {
	id = ko.217
	desc = EVTDESC_KO_089
	picture = GFX_evt_kfeast

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_089
		if = {
			limit = {
				NOT = { trait = stressed }
				NOT = { trait = craven }
			}
			add_trait = stressed
			set_character_flag = bad_chuseok
		}
		if = {
			limit = {
				OR = {
					trait = stressed
					trait = craven
				}
			}
			hidden_tooltip = {
				random_list = {
					10 = {
						if = {
							limit = {
								NOT = {
									has_dlc = "Reapers"
								}
							}
							add_trait = ill
						}
						if = {
							limit = {
								has_dlc = "Reapers"
							}
							add_symptom_effect = yes
						}
						set_character_flag = bad_chuseok
						character_event = { id = 38290 } # Notify
					}
					10 = {
						add_trait = stutter
						set_character_flag = bad_chuseok
						character_event = { id = 38253 } # Notify
					}
					10 = {
						add_trait = depressed
						set_character_flag = bad_chuseok
						character_event = { id = 38288 } # Notify
					}
					10 = {
						add_trait = lunatic
						set_character_flag = bad_chuseok
						character_event = { id = 38304 } # Notify
					}
				}
			}
		}
	}
}

