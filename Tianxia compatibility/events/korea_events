		}
		modifier = {
			factor = 0.5
			trait = cruel
		}
		modifier = {
			factor = 2
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
	}
	
	# All the options are the same; why?

	option = {
		name = EVTOPTA_KO_113.v
		wealth = -10
		set_character_flag = has_tattoo
		spouse = {
			opinion = {
				modifier = opinion_impressed
				who = ROOT
				years = 1
			}
		}
		any_consort = {
			opinion = {
				modifier = opinion_impressed
				who = ROOT
				years = 1
			}
		}	
	}
	
	option = {
