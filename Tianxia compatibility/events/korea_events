		name = EVTOPTB_KO_1006 # Why would you pick this over C?
		tooltip_info = greedy
		trigger = { trait = greedy }
		hidden_tooltip = {
			narrative_event = { id = ko.1067 }
		}
	}
	
	option = {
		name = EVTOPTC_KO_1006
