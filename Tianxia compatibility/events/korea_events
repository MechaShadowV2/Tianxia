# Hairpin
character_event = {
	id = ko.835
	desc = EVTDESC_KO_560.10
	picture = GFX_evt_court_ladies

	is_triggered_only = yes

	immediate = { 		
		random_realm_character = {
			limit = {
				is_female = yes
				dynasty = none
				# Incapable characters, prisoners, etc. can be targeted; is this correct?
			}
		save_event_target_as = female_k_hairpin
		} 
	}
	
	option = {
		name = EVTOPTA_KO_560.10
		event_target:female_k_hairpin  = {
			character_event = { id = ko.836 }
		}
	}
	
	option = {
		name = EVTOPTB_KO_560.10
		random_list = {
			50 = {
				prestige = -50
			} 	
			50 = {}
		}	
	}
	option = {
		name = EVTOPTC_KO_560.10
		tooltip_info = diplomacy
		trigger = { diplomacy = 10 }
		prestige = 100
		event_target:female_k_hairpin  = { 
			restige = -100
		}
	}	
}

