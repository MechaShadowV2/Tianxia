# You fight tiger
character_event = {
	id = ko.052
	desc = EVTDESC_KO_030
	picture = GFX_evt_white_tiger

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_030
		trigger = {
			NOT = { trait = wounded }
			NOT = { trait = maimed }
		}
		hidden_tooltip = {
			if = {
				limit = {
					OR = {
						trait = hunter
						trait = duelist
						trait = berserker
					}
				}
				random_list = {
					80 = { character_event = { id = ko.053 } } 
					20 = { character_event = { id = ko.054 } } 
				}
			}
			if = {
				limit = {
					NOT = { trait = hunter }
					NOT = { trait = duelist }
					NOT = { trait = berserker }
				}
				random_list = {
					50 = { character_event = { id = ko.053 } } 
					50 = { character_event = { id = ko.054 } } 
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_030
		random_list = {
			30 = { add_trait = craven }
			20 = { piety = 20 }
			30 = { prestige = -20 }
			20 = { add_trait = sympathy_pagans }
		}
	}								
}

# Slays Tiger
character_event = {
	id = ko.053
	desc = EVTDESC_KO_034
	picture = GFX_evt_successful_tigerhunt

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_034
		prestige = 50
		#add_trait = tiger_killer # Why? Indian tiger hunts give no trait!
		add_trait = brave
		random_list = {
			2 = {
				add_trait = maimed  # This is not used with Reaper's Due...
				hidden_tooltip = {
					character_event = { id = 38281 }
				}
			}
			5 = {
				add_trait = wounded
				hidden_tooltip = {
					character_event = { id = 38280 }
				}
			}
			50 = {
				add_trait = inspiring_leader
			}
			40 = {
				change_diplomacy = -1
			}
		}
	}
	option = {
		name = EVTOPTB_KO_034
		tooltip_info = proud
		trigger = { trait = proud }
		prestige = 200
		#add_trait = tiger_killer # Why? Indian tiger hunts give no trait!
		add_trait = brave
		hidden_tooltip = {
			character_event = { id = 38270 }
		}
		random_list = {
			2 = {
				add_trait = maimed  # This is not used with Reaper's Due...
				hidden_tooltip = {
					character_event = { id = 38281 }
				}
			}
			5 = {
				add_trait = wounded
				hidden_tooltip = {
					character_event = { id = 38280 }
				}
			}
			50 = {
				add_trait = inspiring_leader
			}
			40 = {
				change_diplomacy = -1
			}
		}
	}		
}

