		name = EVTOPTA_KO_575.2	
		ai_chance = {
			factor = 3
			modifier = {
				factor = 2
				trait = ambitious
			}				
			modifier = {
				factor = 2.0
				trait = zealous
			}			
		}			
		father  = {
			character_event = { id = ko.463 } # This could easily break if the father is dead
			# Also, should this trigger for an imprisoned/incapable/etc. fatheR?
		}
	}
	
	option = {
		name = EVTOPTB_KO_575.2
		ai_chance = {
			factor = 3
			modifier = {
				factor = 2.0
				trait = craven
			}
			modifier = {
				factor = 2.0
				trait = content
			}
			modifier = {
				factor = 100
				trait = skeptical
			}
			modifier = {
				factor = 5
				religion = buddhist
			}				
		}					
	}
	
	option = {
		trigger = { FROM = { is_female = yes } } 
		name = EVTOPTC_KO_575.1
		character_event = { id = ko.895 }
	}				
}

# Court Mu
character_event = {
	id = ko.896
	desc = EVTDESC_KO_576
	picture = GFX_evt_mu_rattle

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_576
		ai_chance = {
			factor = 3
			modifier = {
				factor = 2
				trait = ambitious
			}				
			modifier = {
				factor = 2.0
				trait = zealous
			}				
			modifier = {
				factor = 5
				religion = korean_pagan
			}				
		}
		hidden_tooltip = {
			set_variable = { which = "mufeastlevel" value = 1 }
			character_event = { id = ko.900 days = 10 } #time for ceremony
			any_realm_lord = {
				limit = {
					NOT = { is_inaccessible_trigger = yes }
					prisoner = no
					NOT = { has_character_flag = do_not_disturb }
					NOT = { trait = incapable }
					NOT = { has_character_modifier = holding_large_feast }
					war = no
					is_adult = yes
				}
				letter_event = {
					id = ko.897 #invitations
					days = 2
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_576
		ai_chance = {
			factor = 3
			modifier = {
				factor = 2.0
				trait = craven
			}
			modifier = {
				factor = 2.0
				trait = content
			}
			modifier = {
				factor = 100
				trait = skeptical
			}
			modifier = {
				factor = 5
				religion = buddhist
			}				
		}
	}				
}
 
 #Realm Ruler is invited to celebrate Court Mu 
letter_event = {
	id = ko.897
	desc = EVTDESC_KO_576.1

	is_triggered_only = yes

	trigger = { NOT = { is_inaccessible_trigger = yes } }

	option = {
		name = EVTOPTA_KO_576.1 #Bring a little
		ai_chance = {
			factor = 1
			modifier = {
				factor = 2
				opinion = { who = from value = -40 }
			}
			modifier  = {
				factor = 2
				opinion = { who = from value = -20 }
			}
		}
		hidden_tooltip = { change_variable = { which = "mufeastlevel" value = 1 } }	
		set_character_flag = coming_to_mu_ritual
		FROM = {
			letter_event = {
				id = ko.898
				days = 1
				random = 2
				tooltip = "EVTTOOLTIP72012"
			}
		}		
		
	}
	option = {
		name = EVTOPTB_KO_576.1 #Bring allot
		ai_chance = {
			factor = 1
			modifier = {
				factor = 2
				opinion = { who = from value = 20 }
			}
			modifier  = {
				factor = 2
				opinion = { who = from value = 10 }
			}
		}
		hidden_tooltip = { change_variable = { which = "mufeastlevel" value = 2 } }	
		set_character_flag = coming_to_mu_ritual
		FROM = {
			letter_event = {
				id = ko.898
				days = 1
				random = 7
				tooltip = "EVTTOOLTIP72012"
			}
		}		
	}	
	option = {
		name = EVTOPTC_KO_576.1 #Refuse
		ai_chance = {
			factor = 1
			modifier  = {
				factor = 2
				NOT = { opinion = { who = from value = -60 } }
			}
			modifier  = {
				factor = 2
				NOT = { opinion = { who = from value = -80 } }
			}
			modifier  = {
				factor = 4
				trait = skeptical
			}
			modifier  = {
				factor = 4
				trait = greedy
			}			
		}
		FROM = {
			letter_event = {
				id = ko.899
				days = 1
				random = 2
				tooltip = "EVTTOOLTIP72012"
			}
		}
	}
}

