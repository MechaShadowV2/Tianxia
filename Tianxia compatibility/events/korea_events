# Yongdeungje begins
character_event = {
	id = ko.600
	desc = EVTDESC_KO_600
	picture = GFX_evt_agriculture
	culture = jeju

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_600

		hidden_tooltip = { character_event = { id = ko.601 days = 3 } } # Jesa
		hidden_tooltip = { character_event = { id = ko.660 days = 30 } } # Clearing all flags and modifiers after feast

		set_character_flag = yongdeungje_begun
		hidden_tooltip = {
			random_list = {
				20 = { #Good yongdeungje
					character_event = { id = ko.635 days = 29 }
				}
				20 = { #Bad yongdeungje
					character_event = { id = ko.636 days = 29 }
				}
				20 = { #GOOD yongdeungje
					character_event = { id = ko.637 days = 29 }
				}
			}
			random_list = {
				20 = { #Underwater Kingdom
					character_event = { id = ko.615 days = 9 random = 4}
				}
				20 = { #Henyee
					character_event = { id = ko.608 days = 14 random = 4 }
				}
				20 = { #Dolphin
					character_event = { id = ko.643 days = 10 random = 4 }
				}
				20 = { #Villagers compete
					character_event = { id = ko.642 days = 9 random = 4 } 
				}
				20 = { }
			}
			random_list = {
				20 = { #Water polo
					character_event = { id = ko.644 days = 6 }
				}
				20 = { #Buddhists
					character_event = { id = ko.645 days = 21 }
				}
				20 = { #Jeolla Mayor
					character_event = { id = ko.646 days = 24 }
				}
				20 = { 
					character_event = { id = ko.647 days = 27 } 
				}
				20 = { character_event = { id = ko.280 days = 2 random = 20 }
				}
			}		
			any_courtier = {
				limit = {
					is_adult = yes
					NOT = { trait = incapable }
				}
				set_character_flag = attending_yongdeungje
				hidden_tooltip = {
					character_event = { id = ko.660 days = 31 }
				} 
			}
		}
	}
}


