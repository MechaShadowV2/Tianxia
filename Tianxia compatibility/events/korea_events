# Gonggi
character_event = {
	id = ko.528
	desc = EVTDESC_KO_526
	picture = GFX_evt_yutpan

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_526
		if = {
			limit = {
				OR = {
					trait = quick
					trait = gamer
					trait = genius
				}
			}
			random_list = {
				80 = { character_event = { id = ko.531 } } 
				20 = { character_event = { id = ko.534 } } 
			}
		}
		if = {
			limit = {
				NOT = { trait = quick }
				NOT = { trait = gamer }
				NOT = { trait = genius }
			}
			random_list = {
				50 = { character_event = { id = ko.532 } } 
				50 = { character_event = { id = ko.533 } }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_526
		prestige = 10
		character_event = { id = ko.530} 
	}
	option = {
		trigger = { is_female = no }
		name = EVTOPTC_KO_526
		character_event = { id = ko.529 }
		piety = -20
	}
	option = {
		trigger = { is_female = yes }
		name = EVTOPTD_KO_526
		character_event = { id = ko.529 }
		piety = -20
	}
}

