# Calculating prosperity
character_event = {
	id = ko.1016

	is_triggered_only = yes
	trigger = { has_character_modifier = port_of_calling  }
	hide_window = yes

	immediate = {
		if = {
			limit = {
				OR = {
					is_variable_equal = { which = port_prosperity value = 1 } 
					is_variable_equal = { which = port_prosperity value = 2 } 
					is_variable_equal = { which = port_prosperity value = 3 } 
					is_variable_equal = { which = port_prosperity value = 4 } 
					is_variable_equal = { which = port_prosperity value = 5 } 
				}
			}
			multiply_variable = { which = port_prosperity value = 10 }
			character_event = { id = ko.1017 }
		}
		if = {
			limit = {
				OR = {
					is_variable_equal = { which = port_prosperity value = 6 } 
					is_variable_equal = { which = port_prosperity value = 7 } 
					is_variable_equal = { which = port_prosperity value = 8 } 
					is_variable_equal = { which = port_prosperity value = 9 } 
					is_variable_equal = { which = port_prosperity value = 10 } 
				}
			}
			multiply_variable = { which = port_prosperity value = 10 }
			character_event = { id = ko.1018 }
		}
		if = {
			limit = {
				OR = {
					is_variable_equal = { which = port_prosperity value = 11 } 
					is_variable_equal = { which = port_prosperity value = 12 } 
					is_variable_equal = { which = port_prosperity value = 13 } 
					is_variable_equal = { which = port_prosperity value = 14 } 
					is_variable_equal = { which = port_prosperity value = 15 } 
				}
			}
			multiply_variable = { which = port_prosperity value = 10 }
			character_event = { id = ko.1019 }
		}
		if = {
			limit = {
				OR = {
					is_variable_equal = { which = port_prosperity value = 16 } 
					is_variable_equal = { which = port_prosperity value = 17 } 
					is_variable_equal = { which = port_prosperity value = 18 } 
					is_variable_equal = { which = port_prosperity value = 19 } 
					is_variable_equal = { which = port_prosperity value = 20 } 
				}
			}
			multiply_variable = { which = port_prosperity value = 10 }
			character_event = { id = ko.1020 }
		}
		if = {
			limit = {
				OR = {
					is_variable_equal = { which = port_prosperity value = 21 } 
					is_variable_equal = { which = port_prosperity value = 22 } 
					is_variable_equal = { which = port_prosperity value = 23 } 
					is_variable_equal = { which = port_prosperity value = 24 } 
					is_variable_equal = { which = port_prosperity value = 25 } # Are we certain this cannot be exceeded?
				}
			}
			multiply_variable = { which = port_prosperity value = 10 }
			character_event = { id = ko.1021 }
		}
	}
}
		
# Village 1
character_event = {
	id = ko.1017
	desc = EVTDESC_KO_583.13
	picture = GFX_evt_fishing_village_one

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_583.13
		hidden_tooltip = {
			character_event = { id = ko.1022 } 				
			remove_character_modifier = port_of_calling
			event_target:fishingvillage = {
				remove_province_modifier = trade_port
			} 
		}
		export_to_variable = { # This just overwrites port_prosperity with ROOT's wealth; I'm pretty sure that's not what you want to do...
			which = port_prosperity
			value = wealth
			who = ROOT # This is redundent
		}
	}
}

