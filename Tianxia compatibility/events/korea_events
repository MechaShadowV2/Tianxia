# Counting bounce event
character_event = {
	id = ko.900

	is_triggered_only = yes

	hide_window = yes
	immediate = {
		random_list = {
			10 = { 
				modifier = {
					factor = 100
					is_variable_equal = { which = "mufeastlevel" value = 5 }
				}
				character_event = { id = ko.901 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.902 }
				}
			} 
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "mufeastlevel" value = 5 }
				}
				character_event = { id = ko.903 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.904 }
				}
			} 
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "mufeastlevel" value = 10 }
				}
				character_event = { id = ko.905 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.906 }
				}
			}
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "mufeastlevel" value = 15 }
				}
				character_event = { id = ko.907 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.908 }
				}
			}
			10 = {
				modifier = {
					factor = 100
					is_variable_equal = { which = "mufeastlevel" value = 20 }
				}
				character_event = { id = ko.909 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.910 }
				}
			}
		}
	}
}

