	desc = EVTDESC_KO_350.41
	desc = EVTDESC_KO_350.42
	desc = EVTDESC_KO_350.43
	desc = EVTDESC_KO_350.44
	desc = EVTDESC_KO_350.45
	desc = EVTDESC_KO_350.46
	desc = EVTDESC_KO_350.47
	desc = EVTDESC_KO_350.48
	desc = EVTDESC_KO_350.49
	desc = EVTDESC_KO_350.50
	desc = EVTDESC_KO_350.51
	desc = EVTDESC_KO_350.52

	picture = GFX_evt_mu
	only_playable = yes
	is_triggered_only = yes
	
	trigger = {
		has_character_flag = mu_daughter		
		in_command = no
	}	

	weight_multiplier = {
		days = 1
		modifier = {
			factor = 3
			trait = slothful   
		}
		modifier = {
			factor = 0.5
			trait = diligent  
		}
		modifier = {
			factor = 0.5
			trait = trusting
		}
		modifier = {
			factor = 1.5
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = paranoid
		}		
	}
	
	option = {
		name = EVTOPTA_KO_350.1 # A lot of stuff seems to go into a rather simple outcome; is this intentional?
		piety = 10
	}
}

# Kim Yusin
narrative_event = {
	id = ko.475
	desc = EVTDESC_KO_186
	title = EVTTITLE_KO_186
	picture = GFX_evt_comet

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_186
		hidden_tooltip = {
			narrative_event = { id = ko.476 }
		}
	}
	option = {
		name = EVTOPTB_KO_186
		tooltip_info = learning
		trigger = { learning = 15 }
		hidden_tooltip = {
			narrative_event = { id = ko.476 }
			random_child = {
				random_list = {
					50 = {
						add_character_modifier = {
							name = lost_in_the_stars	
							duration = 60
						}
					}
					30 = {}
					20 = {
						add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
					}
				}
			}
		} 
	}
}


# Kim Yusin 2
narrative_event = {
	id = ko.476
	desc = EVTDESC_KO_187
	title = EVTTITLE_KO_187
	picture = GFX_evt_chuseok

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_187
		hidden_tooltip = {
			narrative_event = { id = ko.477 }
		}		
	}
	option = {
		name = EVTOPTB_KO_187
		random_courtier = {
			limit = {
				trait = drunkard
			}
			opinion = {
				modifier = opinion_carousing
				who = ROOT
				years = 2
			} 		
		}
		hidden_tooltip = {
			narrative_event = { id = ko.477 } 
		}		
	}
	option = { 
		name = EVTOPTC_KO_187
		trigger = { NOT = { dynasty = 1000078000 } } # Why have a dynasty-specific option with the same outcome?
		hidden_tooltip = {
			narrative_event = { id = ko.477 }
		} 
	}
}

# Kim Yusin 3
narrative_event = {
	id = ko.477
	desc = EVTDESC_KO_188
	title = EVTTITLE_KO_188
	picture = GFX_evt_mountain_shrine

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_188
		hidden_tooltip = { 
			narrative_event = { id = ko.478 }
		}		
	}
	option = {
		name = EVTOPTB_KO_188
		tooltip_info = learning
		trigger = { learning = 15 }
		hidden_tooltip = {
			narrative_event = { id = ko.478 } 
		}		
	}
	option = {
		name = EVTOPTC_KO_188
		tooltip_info = poet
		trigger = { trait = poet }
		any_courtier = { # Not random?
			limit = {
				NOT = {
					trait = poet
				}
			}
			opinion = {
				modifier = opinion_bad_poetry
				who = ROOT
				months = 6
			} 		
		}
		hidden_tooltip = {
			narrative_event = { id = ko.478 } 
		}		
	}							
}

# Kim Yusin 4
narrative_event = {
	id = ko.478
	desc = EVTDESC_KO_189
	title = EVTTITLE_KO_189
	picture = GFX_evt_ambushed

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_189
		random_child = {
			random_list = {
				34 = {
					add_trait = just
				}
				33 = {
					add_trait = zealous
				}
				33 = {
					add_trait = paranoid
				}
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.479 }
		}
	}
	option = {
		name = EVTOPTB_KO_189
		trigger = { religion = korean_pagan_reformed }	
		piety = 25
		any_courtier = { limit = { religion = buddhist }
			opinion = {
				modifier = opinion_charge_infidel # Is this the correct modifier?
				who = ROOT
				months = 6
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.479 } 
		}		
	}
	option = {
		name = EVTOPTC_KO_189
		piety = -25
		add_trait = just
		any_courtier = { # Not random?
			limit = {
				trait = just
			} 
			opinion = {
				modifier = opinion_open_minded
				who = ROOT
				months = 6
			} 		
		}
		hidden_tooltip = {
			narrative_event = { id = ko.479 } 
		}		
	}							
}

# Kim Yusin 5
narrative_event = {
	id = ko.479
	desc = EVTDESC_KO_190
	title = EVTTITLE_KO_190
	picture = GFX_evt_gift

	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_190
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
		hidden_tooltip = { 
			random_child = {
				random_list = {
					10 = {
						add_trait = ambitious
					}
					90 = {}
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_190
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}		
}	

# On Go!
narrative_event = {
	id = ko.480
	desc = EVTDESC_KO_321
	title = EVTTITLE_KO_321
	picture = GFX_evt_asian_go_game

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_321
		hidden_tooltip = {
			narrative_event = { id = ko.481 }
		} 
	}							
	
	option = {
		name = EVTOPTB_KO_321
		tooltip_info = learning
		trigger = { learning = 15 }
		hidden_tooltip = {
			narrative_event = { id = ko.481
		}
		random_child = {
			random_list = {
				20 = {
					add_trait = gamer
				}
				60 = {}
				20 = {
					add_trait = curious # This is a Conclave childhood trait, but there is no check for Conclave or being a child!
				}		
				}
			} 
		} 
	}
	option = {
		name = EVTOPTC_KO_321
		hidden_tooltip = {
			character_event = { id = ko.337 }
		} 
	}
}				


# On Go! 2
narrative_event = {
	id = ko.481
	desc = EVTDESC_KO_322
	title = EVTTITLE_KO_322
	picture = GFX_evt_construction_site_korean

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_322
		hidden_tooltip = {
			narrative_event = { id = ko.482	}
		}		
	}
	option = {
		name = EVTOPTB_KO_322
		trigger = { NOT = { dynasty = 1000155023 } }
		random_courtier = {
			limit = {
				dynasty = 1000155023 # There's no check for the existence of a courtier with this dynasty, and I also don't really see why there should be special treatment for a specific dynasty...
			}
			change_intrigue = 3 # Why?
			opinion = {
				modifier = opinion_carousing
				who = ROOT
				years = 2
			} 		
		}
		hidden_tooltip = {
			narrative_event = { id = ko.482 } 
		}		
	}
	option = { 
		name = EVTOPTC_KO_322
		hidden_tooltip = {
			narrative_event = { id = ko.482 } # Why does the same outcome happen for two different options?
		}
	}
}

