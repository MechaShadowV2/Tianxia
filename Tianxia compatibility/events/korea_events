			opinion = {
				modifier = deceitful_neg
				who = ROOT
				years = 1
			}
		}
		add_trait = deceitful
	}
	option = { 
		name = EVTOPTB_KO_539
		tooltip_info = intrigue
		trigger = { intrigue = 10 } # This has no effect; is this intentional?
	}
	option = { 
		name = EVTOPTC_KO_539
		tooltip_info = intrigue
		trigger = { intrigue = 10 } 
		any_voter = {
			opinion = {
				modifier = opinion_satisfied
				who = ROOT
				years = 1
				}
			}
		}		
	}
}

