# Dano Begins
character_event = {
	id = ko.500
	desc = EVTDESC_KO_500
	picture = GFX_evt_feast

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_500
		custom_tooltip = { text = EVTOPTA_KO_500_TT }
		if = {
			limit = { 
				OR = {
					religion = korean_pagan
					religion = korean_pagan_reformed
				}
			} 
			hidden_tooltip = {
				random_list = {
					95 = { #Normal Dano
						character_event = { id = ko.502 days = 5 }
					}
					5 = { #Seoul Dano
						modifier = {
							factor = 60
							has_landed_title = c_hanseong
						}
						character_event = { id = ko.839 days = 9 }
					}
				}
			}
		}
		hidden_tooltip = {
			character_event = { id = ko.589 days = 29 } # End feast
		}
		hidden_tooltip = {
			character_event = { id = ko.590 days = 30 } # Clearing all flags and modifiers after feast
		}
		if = {
			limit = {
				is_female = yes
			}
			hidden_tooltip = {
				character_event = { id = ko.514 days = 16 }
				character_event = { id = ko.501 days = 4 }
			} # sweet flag
		}
		set_character_flag = dano_begun
		hidden_tooltip = {
			random_list = {
				20 = { #Old Peddler
					character_event = { id = ko.512 days = 7 }
				}
				20 = { #Dance troupe Baekje
					character_event = { id = ko.522 days = 11 }
				}
				20 = { #Dance troupe Korean islands
					character_event = { id = ko.523 days = 7 }
				}
				20 = { #Cat Lady
					character_event = { id = ko.524 days = 6 }
				}
				20 = { #SamShe
					character_event = { id = ko.525 days = 7 }
				}
			}
			random_list = {
				10 = { #Fake Goblin
					character_event = { id = ko.526 days = 9 }
				}
				10 = { #Gongi
					character_event = { id = ko.528 days = 9 }
				}
				10 = { #Son/Daughter kissing slave
					character_event = { modifier = { factor = 0 has_children = no }  id = ko.527 days = 10 }
				}
				10 = { #Heavenly armor
					character_event = { id = ko.535 days = 9 }
				}
				10 = {
					character_event = { id = ko.505 days = 5 }
				}
				10 = { #Flowers
					character_event = {  id = ko.845 days = 11 }
				}
				10 = { #Mu selling Jeon
					character_event = {  id = ko.847 days = 8 }
				}
				10 = { #Old lady needs help
					modifier = {
						factor = 0
						is_adult = yes
					}
					character_event = { id = ko.861 days = 9 }
				}
				10 = { #New Clothes
					modifier = {
						factor = 0
						is_adult = yes
					}
					character_event = { id = ko.893 days = 9 }
				}
				10 = { #Mu Cherries
					modifier = {
						factor = 0
						is_adult = yes
					}
					character_event = { id = ko.894 days = 9 }
				}					
				1 = { #Flower Dance
					modifier = {
						factor = 10
						has_job_title = job_treasurer
					}
					character_event = { id = ko.862 days = 4 }
				}
				10 = { #Flower delegate to treasurer
					job_treasurer = {
						character_event = { id = ko.862 days = 4 }
					}
				} 						
			}
			random_list = {
				20 = { #Iranian-korean fusion dance
					character_event = { id = ko.538 days = 6 }
				}
				20 = { #Nampynmdang
					character_event = { id = ko.551 days = 21 }
				}
				15 = { #Meet butchers
					character_event = { id = ko.554 days = 24 }
				}
				15 = {
					character_event = { id = ko.536 days = 20 } #orphans
				}
				15 = {
					character_event = { id = ko.556 days = 12 } #wine festival
				}
				10 = {
					modifier = {
						factor = 0
						is_female = no
					}
					character_event = { id = ko.804 days = 5 random = 6 } # Hwajeonnori
				}
				10 = {
					job_spiritual = {
						character_event = { id = ko.896 days = 4 } #celebrate court mu
					}
				}
				10 = {
					character_event = { id = ko.955 days = 7 } #hwarengi
				}
				1 = {
					modifier = {
						factor = 15
						any_demesne_province = {
							port = yes
						}
					}
					character_event = { id = ko.971 days = 7 }  #Visit Coastal Village 				
				}
			}
			any_courtier = {
				limit = {
					is_adult = yes
					NOT = { trait = incapable }
				}
				set_character_flag = attending_dano
				hidden_tooltip = {
					character_event = { id = ko.590 days = 31 }
				} 
			}
		}
	}
}

