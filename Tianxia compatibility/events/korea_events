# The Story of Silk 3 
narrative_event = {
	id = ko.734
	desc = EVTDESC_KO_406
	title = EVTTITLE_KO_406
	picture = GFX_evt_lunatic

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_406
		trigger = {
			job_marshal  = {
				is_married = no
				age = 45
				is_female = no
			}
		} 
		set_character_flag = daughter_marshal_kdrama
		hidden_tooltip = {
			random_list = {
				50 = { 
					any_child  = {
						limit = {
							is_alive = yes
							is_female = yes
							is_married = no
							age = 14 # I sure hope this isn't leading to tumbling, since that would go against vanilla's consistent "no tumbling before 16" rule
							# Needs checks for homosexuality, imprisonment, etc.
						}
						save_event_target_as = infatuated_daughter
					}
					random_realm_character  = {
						limit = {
							has_job_title = job_marshal
							is_alive = yes
							is_female = no
							is_married = no
							# Probably needs a homosexuality check
						}
						save_event_target_as = infatuated_marshal
					}
				}
				50 = {}
			}
			narrative_event = { id = ko.735 }
		}
	}

	option = {
		name = EVTOPTB_KO_406 # This event option looks extremely boring since it has no visible effect...
		hidden_tooltip = {
			narrative_event = { id = ko.735 }
		}
	}
	
	option = {
		name = EVTOPTC_KO_406
		any_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 7
				NOR = {
					age = 14
					trait = cynical
				}
			}
			opinion = {
				modifier = opinion_narrow_minded
				who = ROOT
				years = 1
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.735 }
		}
	}
}

