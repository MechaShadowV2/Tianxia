	}
	
	option = {
		name = EVTOPTA_KO_418
		
		# The variable changes probably should be hidden...
		change_variable = { which = "their_luv" value = 1 }
				job_marshal = {
				opinion = {
				modifier = opinion_agreed_with_opinion
				who = ROOT
				years = 1
			}	
		}		
 	}
	option = {
		name = EVTOPTB_KO_418
		tooltip_info = intrigue
		trigger = { intrigue = 10 } 
		change_intrigue = 1		
 	}
	option = {
		name = EVTOPTC_KO_418
		character_event = { id = ko.746 days = 800}	
	}
}

