			20 = { character_event = { id = ko.084 days = 30 } }
			20 = { character_event = { id = ko.083 days = 30 } }
			60 = { character_event = { id = ko.085 days = 30 } }
		}
	}
}
	
# Only 1 Man Passes
character_event = {
	id = ko.083
	desc = EVTDESC_KO_053
	picture = GFX_evt_korean_scholar
	culture_group = koreanic
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_053
			random_list = {
				40 = { 
					create_character = {
						random_traits = yes
						dynasty = NONE
						religion = ROOT
						culture = ROOT
						female = no
						age = 27
						trait = administrator
						trait = genius
						trait = fair
						attributes = {
							learning = 10
							stewardship = 10
						}
					}
				}
			60 = {
				piety = 1 # This is an extremely small event reward, especially compared to the other option; is it correct?
			}
		}
	}
	option = {
		name = EVTOPTB_KO_053
		piety = 200
		random_list = {
			83 = {
				add_trait = zealous
			}
			17 = {
				change_diplomacy = -1
			}
		}
	}
	# This doesn't seem complete since the Lustful trigger would seem to indicate that you'd tumble/get a lover
	# There also is no check for homosexuality or adding/removal of that trait for the spawned character to make sure attraction is correct
	option = {
		name = EVTOPTC_KO_053
		tooltip_info = lustful
		trigger = { trait = lustful }
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = no
			age = 27
			trait = administrator
			trait = genius
			trait = fair
			attributes = {
				learning = 10
				stewardship = 10
			}
		}  
	}
}

# Only 1 Woman Passes
character_event = {
	id = ko.084
	desc = EVTDESC_KO_054
	picture = GFX_evt_pray
	culture_group = koreanic
	
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_054
		random_list = {
			40 = {
				create_character = {
					random_traits = yes
					dynasty = NONE
					religion = ROOT
					culture = ROOT
					female = no
					age = 27
					trait = administrator
					trait = genius
					trait = fair
					attributes = {
						learning = 10
						stewardship = 10
					}
				} 
			}
			60 = {
				piety = 1 # This is an extremely small event reward, especially compared to the other option; is it correct?
			}
		}
	}
	option = {
		name = EVTOPTB_KO_054
		piety = 200
		random_list = {
			83 = {
				add_trait = zealous }
			17 = {
				change_diplomacy = -1
			}
		}
	}	
	
	# This doesn't seem complete since the Lustful trigger would seem to indicate that you'd tumble/get a lover
	# There also is no check for homosexuality or adding/removal of that trait for the spawned character to make sure attraction is correct
	option = {
		name = EVTOPTC_KO_054
		trigger = { trait = lustful }
		tooltip_info = lustful
		create_character = {
			random_traits = yes
			dynasty = NONE
			religion = ROOT
			culture = ROOT
			female = yes
			age = 24
			trait = administrator
			trait = genius
			trait = fair
			attributes = {
				learning = 10
				stewardship = 10
			}
		}  
	}
}

# Conclusion
character_event = {
	id = ko.085
	desc = EVTDESC_KO_055
	picture = GFX_evt_chinese_characters
	culture_group = koreanic
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_055
		add_trait = proud					
	}
	option = {
		name = EVTOPTB_KO_055
		capital_scope = { 
			add_province_modifier = {
				name = scholar_foundation_province
				days = 2000
			} 				
		}				
	}
	option = {
		name = EVTOPTC_KO_055
		character_event = { id = ko.084 }
		set_character_flag = reforming_alphabet_japanese_lines
	}
	option = {
		name = EVTOPTD_KO_055
		set_character_flag = reforming_alphabet
		character_event = { id = ko.086 days = 14 }								
	}
}

# Decide to reform the alphabet
character_event = {
	id = ko.086
	desc = EVTDESC_KO_057
	picture = GFX_evt_chinese_characters
	culture_group = koreanic
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_057
		character_event = { id = ko.087 days = 55 } }
		
		option = {
		name = EVTOPTB_KO_057
		create_character = {
			random_traits = no
			dynasty = NONE
			religion = buddhist
			culture = bengali
			female = no
			age = 50
			attributes = {
				diplomacy = 12
				stewardship = 10
				intrigue = 11
				learning = 14
			}
			trait = genius
			trait = scholar
			trait = patient
			trait = mastermind_theologian
		}
		new_character = {
			save_event_target_as = language_guru
		}
		character_event = { id = ko.088 days = 30 }
		random_list = {
			83 = {}
			17 = {
				character_event = { id = ko.089 days = 7 }
			}
		}
	}
	
	option = {
		name = EVTOPTC_KO_057
		character_event = { id = ko.087 days = 30 }	#temp
	}
	option = {
		name = EVTOPTD_KO_057
		character_event = { id = ko.087 days = 30 }	#temp						
	}
}

