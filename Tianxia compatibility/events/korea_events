# Merchant Writing a complain bounce event
character_event = {
	id = ko.1002

	is_triggered_only = yes
	trigger = { has_character_modifier = port_of_calling  }
	hide_window = yes

	immediate = {
		random_realm_character = {
			limit = {
				stewardship = 15
				NOR = {
					tier = king
				    tier = emperor
				}
			}
			character_event = { id = 1003 }		
		}
	}
}		

# Preeminent Merchant complains
character_event = {
	id = ko.1003
    desc = {
        text = EVTDESC_KO_583.81 #Hates the character
        trigger = { NOT = { opinion = { who = FROM value = 0 } }}
    }
    desc = {
        text = EVTDESC_KO_583.82 #Likes the character
        trigger = { opinion = { who = FROM value = 0 } }
    }
	picture = GFX_evt_recieve_letter_korean

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_583.81
		FROM = {
			character_event = { id = 1004 }
		}	
	}
	option = {
		name = EVTOPTB_KO_583.81 # This has no effect; why?
	}			
}
		
# Preeminent Merchant complains
