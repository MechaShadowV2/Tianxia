	picture = GFX_evt_sun_temple

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_115
		hidden_tooltip = {
			narrative_event = { id = ko.375 }
			random_child = {
				limit = {
					is_female = yes
					is_alive = yes
					age = 7
					NOR = {
						age = 14
						trait = cynical
						trait = weak
					}
				}
				character_event = { id = ko.460 days = 800 }
			} 			 
		}			
	}		
	
	option = {
		tooltip_info = cynical
		trigger = { trait = cynical }
		prestige = 20
		name = EVTOPTB_KO_115
		hidden_tooltip = {
			narrative_event = { id = ko.375 }
		}
	}
}


