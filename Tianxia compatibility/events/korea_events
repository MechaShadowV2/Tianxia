	id = ko.1059
	
	# Multiple descs without triggers; fix this!
	
	desc =  EVTDESC_KO_908.63
	desc =  EVTDESC_KO_908.64
	desc =  EVTDESC_KO_908.65
	desc =  EVTDESC_KO_908.66
	desc =  EVTDESC_KO_908.67
	
	picture = GFX_evt_mu
	is_triggered_only = yes


	option = {
		name = EVTOPTA_KO_114
		custom_tooltip = { text = EVTOPTA_KO_114_TT }
		set_character_flag = mu_in_the_house
		character_event = { id = ko.331 }					
	}
	option = {
		name = EVTOPTB_KO_114
		custom_tooltip = { text = EVTOPTB_KO_114_TT }
		set_character_flag = story_pend
		narrative_event = { id = ko.373 }								
	}
	option = {
		name = EVTOPTC_KO_114
		custom_tooltip = { text = EVTOPTC_KO_114_TT }
		trigger = { is_adult = yes } 
		character_event =  { id = ko.417 }	
	}
	option = {
		name = EVTOPTD_KO_114
		custom_tooltip = { text = EVTOPTD_KO_114_TT }
		clr_character_flag = do_not_disturb
		clr_character_flag = story_pend
		clr_character_flag = mu_in_the_house
	} 	
}	
	
# Princess Cheonmyeong 1
narrative_event = {
	id = ko.1060
	desc = EVTDESC_KO_1000
	title = EVTTITLE_KO_1000
	picture = GFX_evt_stone_church

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_1000
		hidden_tooltip = { 
			narrative_event = { id = ko.1061 }
		}
	}
	option = {
		name = EVTOPTB_KO_1000
		tooltip_info = learning
		trigger = { learning = 10 }
		random_child = {
			limit = {
				is_alive = yes
				trait = curious # This is a Conclave childhood trait, but there is no check for Conclave!
			}
			opinion = {
				modifier = opinion_impressed
				who = ROOT
				years = 1
			}		
		}
		hidden_tooltip = {
			narrative_event = { id = ko.1061 }
		}
	}
}

# Princess Cheonmyeong 2
narrative_event = {
	id = ko.1061
	desc = EVTDESC_KO_1001
	title = EVTTITLE_KO_1001
	picture = GFX_evt_marriage

	is_triggered_only = yes
	
	option = {
		name = EVTOPT_KO_1001
		hidden_tooltip = {
			narrative_event = { id = ko.1062 }
		}
	}
	option = {
		name = EVTOPTB_KO_1001 # Why wouldn't you pick this?
		random_child = {
			limit = {
				trait = stutter
				is_alive = yes
				age = 6
				NOT = { age = 18 }
			}
			remove_trait = stutter # This is a congenial trait; why is it being removed?
		}
		hidden_tooltip = {
			narrative_event = { id = ko.1062 }
		}
	}
	option = {
		name = EVTOPTC_KO_1001 # This is the same as A
		hidden_tooltip = {
			narrative_event = { id = ko.1062 }
		}
	}
}

# Princess Cheonmyeong 3
narrative_event = {
	id = ko.1062
	desc = EVTDESC_KO_1002
	title = EVTTITLE_KO_1002
	picture = GFX_evt_family

	is_triggered_only = yes
	immediate = {
		random_child = {
			limit = {
				is_alive = yes
				is_female = no
				age = 9
				NOR = {
					age = 20
				}
			}
			save_event_target_as = brotherlyfriend
		}
	}
	
	option = {
		name = EVTOPT_KO_1002
		hidden_tooltip = {
			narrative_event = { id = ko.1063 }
		}
		random_child = {
			limit = {
				is_female = no
				is_alive = yes
				age = 6
				NOT = { age = 18 }

			}
			add_trait = kind	
		}
	}
	option = {
		name = EVTOPTB_KO_1002
		random_child = {
			add_friend = event_target:brotherlyfriend						
			limit = {
				is_female = no
				is_alive = yes
				age = 7
				NOR = {
					age = 15
					trait = cynical
				}
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.1063 }
		}
	}
	
	option = {
		name = EVTOPTC_KO_1002
		trigger = { NOT = { dynasty = 1000078000 } } # Why is there a dynasty-specific option?
		hidden_tooltip = {
			narrative_event = { id = ko.1063 }
		}
	}
}

# Princess Cheonmyeong 4
narrative_event = {
	id = ko.1063
	desc = EVTDESC_KO_1003
	title = EVTTITLE_KO_1003
	picture = GFX_evt_death

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_1003
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	
	option = {
		name = EVTOPTB_KO_1003
		random_child = {
			limit = {
				is_alive = yes
				age = 7
				NOR = {
					age = 16
				}
			}
			random_list = {
				25 = {
					add_trait = cynical
				}
				50 = {}
				25 = {
					add_trait = lustful
				} 
			}		
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 } 
			}
		}
	}
	option = {
		name = EVTOPTC_KO_1003
		random_child = {
			limit = {
				is_betrothed = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			remove_trait = trusting	
		}			
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}

