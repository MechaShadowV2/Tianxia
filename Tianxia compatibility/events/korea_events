# On Go! 5
narrative_event = {
	id = ko.484
	desc = EVTDESC_KO_325
	title = EVTTITLE_KO_325
	picture = GFX_evt_beheading

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_325
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
					random_child = {
						random_list = {
							10 = {
								add_trait = craven
							}
							90 = {}
						}
					}
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_325
		trigger = { NOT = { dynasty = 1000078000 } }
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			} 
		}
	}
	option = {
		name = EVTOPTC_KO_325
		trigger = { dynasty = 1000078000 } # Why have dynasty-specific options if they're the same when it comes to the outcome?
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
		}
	}
	option = {
		name = EVTOPTD_KO_325 # This is the same as option A; why?
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
			random_child = {
				random_list = {
					10 = {
						add_trait = craven
					}
					90 = {}
				}
			}
		}
	}		
}

# The life of Talhae
narrative_event = {
	id = ko.485
	desc = EVTDESC_KO_326
	title = EVTTITLE_KO_326
	picture = GFX_evt_fishing

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_326
		hidden_tooltip = {
			narrative_event = { id = ko.486 }
		} 
	}							
	
	option = {
		name = EVTOPTB_KO_326 # This is the same as A; why?
		hidden_tooltip = {
			narrative_event = { id = ko.486 }
		}
	}
	option = {
		name = EVTOPTC_KO_326
		hidden_tooltip = {
			narrative_event = { id = ko.486 }
		}
		random_child = {
			random_list = {
				20 = {
					add_trait = zealous
				}
				50 = { 
					opinion = {
						modifier = opinion_narrow_minded
						who = ROOT
						years = 2
					} 
				}
				30 = {}		
			}
		} 
	}
	option = {
		tooltip_info = learning
		trigger = { learning = 20 } # This is a VERY high value...
		name = EVTOPTD_KO_326 # Why is there a high-learning option that leads to the same outcome as the first two options?
		hidden_tooltip = { 
			narrative_event = { id = ko.486 }
		} 
	}			
}


# The life of Talhae 2
narrative_event = {
	id = ko.486
	desc = EVTDESC_KO_327
	title = EVTTITLE_KO_327
	picture = GFX_evt_marriage

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_327
		tooltip_info = intrigue
		trigger = { intrigue = 15 }
		
		random_child = {
			random_list = {
				20 = {
					add_trait = paranoid
				}
				50 = {
					change_intrigue = 1
				}
				30 = {}		
			}
		} 
		hidden_tooltip = {
			narrative_event = { id = ko.487 }
